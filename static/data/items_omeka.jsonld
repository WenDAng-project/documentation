		"write:description": [
			{
				"type": "literal",
				"property_id": 560,
				"property_label": "description",
				"is_public": true,
				"@value": "In this poem, Li He 李贺 (790-816) describes the day of youth about to twilight through the image of peach blossoms falling like red rain and invites people to spend their last happy years of youth in the company of wine and entertainment. The poem evokes the dualism between life and death and between youth and old age.",
				"@language": "en"
			}
		],
		"write:trascription-or-translation": [
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "琉璃钟，琥珀浓，小槽酒滴真珠红。烹龙炮凤玉脂泣，罗帏绣幕围香风。吹龙笛，击鼍鼓；皓齿歌，细腰舞。况是青春日将暮，桃花乱落如红雨。劝君终日酩酊醉，酒不到刘伶坟上土！",
				"@language": "zh"
			},
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "Liu li zhong,  hu po nong, xiao cao jiu di zhen zhu hong. peng long pao feng yu zhi qi,  luo wei xiu mu wei xiang feng. chui long di, ji tuo gu; hao chi ge, xi yao wu. kuang shi qing chun ri jiang mu, tao hua luan luo ru hong yu. quan jun zhong ri ming ding zui, jiu bu dao liu ling fen shang tu!",
				"@language": "pny"
			},
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "Glass bell, thick amber, small trough wine dripping with pearl red. Cooking dragon cannon and phoenix jade fat weeping, Luo Wei embroidery curtain surrounds fragrance. \nPlay the dragon flute, beat the drums, sing with white teeth, dance with a slender waist. \nIn fact, the day of youth is about to twilight, and the peach blossoms are falling like red rain. \nI advise you to be drunk all day long, and the wine will not reach Liu Ling's grave. \n\n",
				"@language": "en"
			}
		],
