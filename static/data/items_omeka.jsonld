ype": "literal", "property_id": 570, "property_label": "transcription and translation", "is_public": true, "@value": "W\u00e8i ch\u016bt\u01d4 qi\u00e1n xi\u0101n y\u01d2u ji\u00e9, \nl\u00edngy\u00fan sh\u0113n ch\u00f9 z\u01d2ng x\u016bx\u012bn", "@language": "pny"}, {"type": "literal", "property_id": 570, "property_label": "transcription and translation", "is_public": true, "@value": "Buried in the earth it already has a culm,\nreaching into the sky it is still open-minded.\n(Traslator: Shinewind 2012)\n", "@language": "en"}, {"type": "literal", "property_id": 570, "property_label": "transcription and translation", "is_public": true, "@value": "Nocchieruto, integro, ritto e retto, \nancor prima di germogliare.\nCarezza le nuvole, penetra il profondo, \nma inamovibile onesto rimane.", "@language": "it"}], "wd:P144": [{"type": "resource:item", "property_id": 537, "property_label": "based on", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/122", "value_resource_id": 122, "value_resource_name": "items", "url": null, "display_title": "On Bamboo  (\u7af9)"}, {"type": "resource:item", "property_id": 537, "property_label": "based on", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/123", "value_resource_id": 123, "value_resource_name": "items", "url": null, "display_title": "Ode to Bamboo (\u548f\u7af9)"}], "wd:P8570": [{"type": "customvocab:35", "property_id": 588, "property_label": "front and back matter", "is_public": true, "@value": "No", "@language": "en"}], "wd:P180": [{"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q27891820", "o:label": "bamboo"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q1208000", "o:label": "Junzi"}], "write:tag_of": [{"type": "resource:item", "property_id": 551, "property_label": "tag of", "is_public": true, "@id": "https://writeproject.dipint