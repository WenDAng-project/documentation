		"write:description": [
			{
				"type": "literal",
				"property_id": 560,
				"property_label": "description",
				"is_public": true,
				"@value": "The author of the poem is the Song poet Xu Tingyun 徐庭筠 (1095-1179). This poem is inspired by the bamboo. The bamboo symbolizes the qualities of the perfect literati (junzi). ",
				"@language": "en"
			}
		],
		"write:trascription-or-translation": [
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "不論臺閣與山林，\n愛爾豈惟千畝陰。\n未出土時先有節，\n便凌雲去也無心。\n葛陂始與龍俱化，\n嶰谷聊同鳳一吟。\n月朗風清良夜永，\n可憐王子獨知音。\n",
				"@language": "zh"
			},
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "Bùlùn tái gé yǔ shānlín, \nài ěr qǐ wéi qiānmǔ yīn. \nWèi chūtǔ shí xiān yǒu jié, \nbiàn língyún qù yě wúxīn. \nGé bēi shǐ yǔ lóng jù huà, \nxiè gǔ liáo tóng fèng yī yín. \nYuè lǎng fēng qīng liángyè yǒng, \nkělián wángzǐ dú zhīyīn.",
				"@language": "pny"
			},
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": " No matter what the terrace or the mountains, \nI love you more than a thousand acres of shade.\nBefore they emerge from the earth, they have a knack for it, \nand even when they leave the clouds, they have no heart.\nThe Ge Pei has been transformed with the dragon, \nthe valley with the phoenix.\nThe moon is clear, the wind is clear, and the night is eternal, \nand the prince is alone in knowing his voice.",
				"@language": "en"
			}
		],
