ation", "is_public": true, "@value": "\u5076\u7136\u7684\u547c\u558a\n\u52fe\u8d77\u7121\u9650\n\u9752\u5c71\u7da0\u6c34\n\u6df7\u6210\u4e00\u7247\u7684\n\u975c\u9ed8", "@language": "zh"}, {"type": "literal", "property_id": 570, "property_label": "transcription and translation", "is_public": true, "@value": "\u01d1ur\u00e1n de h\u016bh\u01cen \ng\u014du q\u01d0 w\u00faxi\u00e0n \nq\u012bngsh\u0101n l\u01dc shu\u01d0 \nh\u01d4nch\u00e9ng y\u012bpi\u00e0n de\n j\u00ecngm\u00f2", "@language": "pny"}], "wd:P50": [{"type": "uri", "property_id": 450, "property_label": "author", "is_public": true, "@id": "https://www.wikidata.org/entity/Q111356899", "o:label": "Fung Ming Chip"}], "wd:P571": [{"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "1997", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/582", "@type": ["o:Item", "wdt:Q4502142"], "o:id": 582, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/147", "o:id": 147}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/8", "o:id": 8}, "o:thumbnail": null, "o:title": "Black on black Script, Landscape B (\u5927\u5730\u9ed1\u4e0a\u9ed1\u5b57)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/3df1df70ff98cc94373c0723d2c76646b5d0bcd7.jpg", "medium": "https://writeproject.dipintra.it/files/medium/3df1df70ff98cc94373c0723d2c76646b5d0bcd7.jpg", "square": "https://writeproject.dipintra.it/files/square/3df1df70ff98cc94373c0723d2c76646b5d0bcd7.jpg"}, "o:created": {"@value": "2023-03-16T10:49:58+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:24:57+00:00", "@type": "http://www.w3.org/2001/XMLSchema#