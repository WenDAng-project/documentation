[], "write:attributed-name": [{"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "CWU 2 Olympic Beijing (calligraphic inscription)"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This Calli-Writing Unit is the second inscription, on the upper right-hand corner. It is a long calligraphy composed of 36 columns of Chinese characters written in a highly stenographic cursive style (caoshu \u8349\u4e66), in which the strokes of the characters are merged sometimes, impairing their recognizability. It is written in vertical columns to be read from right to left and from top to bottom, as is customary in classical Chinese, and is embellished by a pronounced dripping effect, which seems to simulate ink drippings on paper. The calligraphic style (caoshu), the variation of the width of the strokes, the arrangement of the text in vertical columns (from top to bottom), and the reading direction (from right to left) follow the traditional calligraphic manner and recalls the strict rules of Chinese traditional calligraphy, but a widespread \u2018dripping\u2019 effect underlines the graffiti taste.", "@language": "en"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q42948", "o:label": "wall"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q2066608", "o:label": "spray painting"}], "wd:P2079": [{"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q659495", "o:label": "aerosol spray"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}], "write:background-color": [{"type": "uri