o:site": [], "write:attributed-name": [{"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "CWU Ten Thousand Birds Startled by the Dawn Drums (\u4e07\u70b9\u57ce\u9e1f\u60ca\u66d9\u9f13)", "@language": "en"}, {"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "Wan dian cheng niao jing shu gu", "@language": "pny"}, {"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "\u4e07\u70b9\u57ce\u9e1f\u60ca\u66d9\u9f13", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This CWU is part of the work \"Ten Thousand Birds Startled by the Dawn Drums (Wan dian cheng niao jing shu gu de shuxie danwei \u4e07\u70b9\u57ce\u9e1f\u60ca\u66d9\u9f13)\" executed by Wei Ligang in 2011. In this work Wei Ligang, at least in part, moves away from any calligraphic register recognizable at first sight, to explore a different kind of composition which looks as if it was informed both by the modernist geometrical abstraction and also by some other figurative imagery and primitive decorative motives. In reality, this operation is part of nearly thirty years of artistic exploration, during which Wei Ligang has been creating a universal abstract language based on the lines and spatial structures of Chinese characters. This language attempts to accommodate the complexity of a variety of western and eastern elements. In his so-called \"Wei's Magic Block\" he deconstructs Chinese characters and incorporates image elements, tracing back to the origin of pictographs. The choice of the large round canvas and the mixed media technique (ink and acrylic) establishes another parallel between the eastern tradition (in which the round support is a classic choice but mainly on paper and silk) and the work of several western lyrical and geometrical abstract artists. The choice of another eastern classical element, the palette, black on golden-brown, together with the homogeneous thickness of the line contribute to highlight the hybridity of this work, which is then further re-elaborate with reversed colors on xuan paper mounted in the form of a large screen (as in the work \"Fluttering Butterflies and Buzzing Bees, a Pleasure Boat and Orchids at Dawn\", Die luan feng xuan lan xiao hua ge \u8776\u4e71\u8702\u55a7 \u5170\u6653\u753b\u8238, 2016). We could also find a visual reference for the shapes and the character of the Nazca Lines, a collection of giant geoglyphs\u2014designs or motifs etched into the ground\u2014located in the Peruvian coastal plain about 250 miles (400 kilometers) south of Lima, Peru. There are three basic types of Nazca Lines: straight lines, geometric designs and pictorial representations. The Nazca are large environmental artworks only visible from the sky, despite the obvious differences in terms of context, size and technicality.", "@language": "en"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q127418", "o:label": "ink"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q207849", "o:label": "acrylic paint"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q4259259", "o:label": "canvas"}], "wd:P2079": [{"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q614467", "o:label": "brush"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}], "write:background-color": [{"type": "uri", "property_id": 561, "property_label": "background color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q897", "o:label": "gold"}], "write:calligraphic-line": [{"type": "customvocab:25", "property_id": 555, "property_label": "calligraphic line", "is_public": true, "@value": "Present"}], "write:significance": [{"type": "customvocab:24", "property_id": 556, "property_label": "significance", "is_public": true, "@value": "Set of lines (meaningless)"}], "wd:P180": [{"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3393049", "o:label": "juxtaposition"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q25496059", "o:label": "overlying stratum"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q18699055", "o:label": "repetition"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q752641", "o:label": "asymmetry"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q2620036", "o:label": "Nazca Lines"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/578", "@type": ["o:Item", "wdt:Q215627"], "o:id": 578, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/125", "o:id": 125}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/5", "o:id": 5}, "o:thumbnail": null, "o:title": "Fung Ming Chip \u00a0\u51af\u660e\u79cb", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/9cb9a384c19c3b95df9f12cb96461c28a363435b.jpg", "medium": "https://writeproject.dipintra.it/files/medium/9cb9a384c19c3b95df9f12cb96461c28a363435b.jpg", "square": "https://writeproject.dipintra.it/files/square/9cb9a384c19c3b95df9f12cb96461c28a363435b.jpg"}, "o:created": {"@value": "2023-03-16T09:19:43+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:21:00+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/719", "o:id": 719}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1172", "o:id": 1172}], "o:site": [], "wd:P2561": [{"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "Fung Ming Chip \u00a0\u51af\u660e\u79cb", "@language": "en"}, {"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "Feng Mingqiu", "@language": "pny"}, {"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "\u00a0\u51af\u660e\u79cb", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "Fung Ming Chip\u00a0\u51af\u660e\u79cb was born in Guangdong in 1951 and moved to Hong Kong in 1956 where he received only primary education before moving to New Jersey in 1977. His passion for seal carving started in New York in 1975, and this allowed him to broaden his horizons subsequently to calligraphy, studying as a self-taught. He moved to Taiwan in 1986 and finally in 2006 to Hong Kong, where he now lives and works; there he opened his art studio \u201cSibuguan\u00a0\u56db\u00a0\u4e0d\u7ba1 Four no studio\u201d in an area of the city called Sai Ying Pun.\nFung Ming Chip is a poet, a seal carver and a calligrapher.\nHis seal production can be divided into three phases: 1) carving stone seals on the basis of traditional models; 2) carving wooden boards; 3) creation of three-dimensional bronze sculptures. At a later time, Fung Ming Chip decided to study and practice calligraphy in order to discover everything that was hidden inside it, seeking its deep meaning and interpreting calligraphy as an artistic medium. In 1996 Fung Ming Chip began experimenting with calligraphy, creating over one hundred new calligraphic \u201cscript types\u201d (xiefa \u5199\u6cd5). These new \u201cscript types\u201d have their own visual language and creative process, breaking with traditional concepts of calligraphy and allowing the viewer to observe calligraphy through a new perspective. The calligraphic production of Fung Ming Chip can be categorized according to three parameters: 1) the degree of legibility of the transcribed text, 2) the use of ink and 3) the spatial arrangement.\nAmong his many international exhibitions, Fung had a major solo retrospective at the Taipei Fine Arts Museum in 1999. He was artist in residence at Cambridge University, and his work is included in major private and institutional collections around the world, including The Metropolitan Museum of Art, New York, White Rabbit Collection, Sydney and Israel Museum, Jerusalem. He has also been active as a painter, photographer, and sculptor. In 2008, he created Les Poemes du Mandarin tableware pattern for Hermes.\nThe calligraphic production of Fung Ming Chip can be categorized according to three parameters: 1) the degree of legibility of the transcribed text, 2) the use of ink and 3) the spatial arrangement.\n1. Legibility: Fung Ming Chip creates artworks in which characters are completely, partially or not recognizable. In the first case, the artist does not detach himself from traditional calligraphy, he traces the characters in full; in the second case characters are partially traced, even if it is still possible to reconstruct them and/or extrapolate their meaning; in the third case the legibility of the characters is compromised, because they are not recognizable, it only remains signs of the characters, approaching a more abstract art.\n2. The use of ink: by means of the dilution of ink, the artist creates new calligraphic \u201cscript types\u201d. By using brush, water, ink and paper, he tries to experiment with different techniques with the aim of stimulating the viewer mind.\n3. The spatial arrangements: Fung Ming Chip breaks down the setting traditional spatial arrangement, exploring new possibilities to expand or reshape the \u201cspace and time\u201d elements present in his art.\nThese three categories are not used always separately, each of them creates new calligraphic \u201cscript types\u201d that sometimes are used together in the same work. These continuous experiments demonstrate the intent of Fung Ming Chip to always go further, through his creativity and imagination.", "@language": "en"}], "write:wikidata-resource": [{"type": "uri", "property_id": 567, "property_label": "wikidata resource", "is_public": true, "@id": "https://www.wikidata.org/entity/Q111356899", "o:label": "Fung Ming Chip"}], "wd:P21": [{"type": "uri", "property_id": 406, "property_label": "gender", "is_public": true, "@id": "https://www.wikidata.org/entity/Q6581097", "o:label": "male"}], "wd:P27": [{"type": "uri", "property_id": 413, "property_label": "country of citizenship", "is_public": true, "@id": "https://www.wikidata.org/entity/Q148", "o:label": "People's Republic of China"}], "wd:P569": [{"type": "numeric:timestamp", "property_id": 407, "property_label": "date of birth", "is_public": true, "@value": "1951", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "wd:P19": [{"type": "uri", "property_id": 408, "property_label": "place of birth", "is_public": true, "@id": "https://www.wikidata.org/entity/Q15175", "o:label": "Guangdong"}], "wd:P106": [{"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q483501", "o:label": "artist"}, {"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3303330", "o:label": "calligrapher"}], "wd:P103": [{"type": "uri", "property_id": 583, "property_label": "native language", "is_public": true, "@id": "https://www.wikidata.org/entity/Q7850", "o:label": "Chinese"}], "wd:P1412": [{"type": "uri", "property_id": 412, "property_label": "languages spoken, written or signed", "is_public": true, "@id": "https://www.wikidata.org/entity/Q52930727", "o:label": "English"}, {"type": "uri", "property_id": 412, "property_label": "languages spoken, written or signed", "is_public": true, "@id": "https://www.wikidata.org/entity/Q7850", "o:label": "Chinese"}], "write:calligraphic-formation": [{"type": "customvocab:21", "property_id": 497, "property_label": "has calligraphic formation", "is_public": true, "@value": "Self-Taught", "@language": "en"}], "write:relevant-activity-location": [{"type": "uri", "property_id": 545, "property_label": "relevant activity location", "is_public": true, "@id": "https://www.wikidata.org/entity/Q8646", "o:label": "Hong Kong"}, {"type": "uri", "property_id": 545, "property_label": "relevant activity location", "is_public": true, "@id": "https://www.wikidata.org/entity/Q60", "o:label": "New York City"}, {"type": "uri", "property_id": 545, "property_label": "relevant activity location", "is_public": true, "@id": "https://www.wikidata.org/entity/Q1867", "o:label": "Taipei"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/579", "@type": ["o:Item", "wdt:Q4502142"], "o:id": 579, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/147", "o:id": 147}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/8", "o:id": 8}, "o:thumbnail": null, "o:title": "Scattered Needle Script, Looking through the Window (\u5076\u7136\u9488\u76f4 \u6563\u5b57)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/acd73d64ec69044cd73c772fd5c4b93bcd8c4d8b.jpg", "medium": "https://writeproject.dipintra.it/files/medium/acd73d64ec69044cd73c772fd5c4b93bcd8c4d8b.jpg", "square": "https://writeproject.dipintra.it/files/square/acd73d64ec69044cd73c772fd5c4b93bcd8c4d8b.jpg"}, "o:created": {"@value": "2023-03-16T10:14:32+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:24:57+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/662", "o:id": 662}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1173", "o:id": 1173}], "o:site": [], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Scattered Needle Script, Looking through the Window (\u5076\u7136\u9488\u76f4 \u6563\u5b57)", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Ou ran zhen zhi san zi ", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "\u5076\u7136\u9488\u76f4 \u6563\u5b57", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "Fung Ming Chip created this artwork starting from the poem \u201cLooking through the Window\u201d Chuang wai wang \u7a97\u5916\u671b. He didn\u2019t write the entire characters of the poem but only some strokes/lines of the characters, so that the characters are no longer recognizable and the text is unreadable. He wrote these strokes in columns from right to left as in traditional calligraphy. He called this type of script \"scattered needle script\" (ou ran zhen \u5076\u7136\u9488).", "@language": "en"}], "wd:P170": [{"type": "resource:item", "property_id": 429, "property_label": "creator", "is_public": true, "@id": "https://writeproject.dipintra.it/api/ite