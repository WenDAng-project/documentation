, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/143", "o:id": 143}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/12", "o:id": 12}, "o:thumbnail": null, "o:title": "CWU 2 Shanshui PIC (\u89c0\u97f3)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/f7c5217bdc30c586522d9cb3d0ae37114fbf634c.jpg", "medium": "https://writeproject.dipintra.it/files/medium/f7c5217bdc30c586522d9cb3d0ae37114fbf634c.jpg", "square": "https://writeproject.dipintra.it/files/square/f7c5217bdc30c586522d9cb3d0ae37114fbf634c.jpg"}, "o:created": {"@value": "2023-02-07T17:37:01+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-14T11:52:44+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/412", "o:id": 412}], "o:item_set": [], "o:site": [], "write:attributed-name": [{"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "CWU 2 Shanshui PIC (\u89c0\u97f3)", "@language": "en"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This Calli-Writing Unit is the first inscription in the centre of the scroll. It is written in \"big characters\" (dazi \u5927\u5b57) and in regular script (kaishu \u6977\u4e66). The first calligraphy is composed of only two traditional Chinese characters, Guan Yin \u89c0\u97f3, the Chinese name of the crew. As it is usual in Chinese calligraphic work, they are written vertically (from top to bottom).\nIn the two big characters, the variation of the width of the strokes produced by a skillful use of the spray-painting is a contemporary expedient to create the illusion of being in front of a classical calligraphy written with the ink brush. Furthermore, if we look at the arrangement of this written section, we will notice that it also reproduces a traditional calligraphic pattern: the main text (the two big characters) is in the center of the work, occupying a commanding position, while the inscriptions with the name of the artists are written on the sides in smaller size (even if horizontally and in alphabetic letters).\nThe name of the crew is significant because it clearly reveals the deep meaning of their art: \u2018Kwan-yin\u2019 explicitly refers to the \u2018Bodhisattva Guanyin\u2019 (Guanyin Pusa \u89c2\u97f3\u83e9\u8428), one of the most popular and well known deity in China. The crew itself points out that the story of the Bodhisattva was fundamental for the choice of their name: Guanyin, in fact, was introduced into China via the Silk Road and was initially presented in a male form, but when Buddhism rooted in China, the Bodhisattva was sinicized and subsequently transformed into a Chinese female form, which later became extremely popular all over the country. Kwanyin Clan\u2019s artistic aim is to present graffiti art in China as an alter ego of the Bodhisattva Guanyin: as it had happened for Guanyin, graffiti art came from abroad, but if it wants to become popular in China it has to sinicize its forms, undergoing deity own metamorphosis.", "@language": "en"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q2066608", "o:label": "spray painting"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q55439", "o:label": "panel painting"}], "wd:P2079": [{"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q659495", "o:label": "aerosol spray"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}], "write:backgro