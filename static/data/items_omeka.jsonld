		"thumbnail_display_urls": {
			"large": "https://writeproject.dipintra.it/files/large/eec9b74f54a55f4d1fa1eec9bd924a61a2ec384f.jpg",
			"medium": "https://writeproject.dipintra.it/files/medium/eec9b74f54a55f4d1fa1eec9bd924a61a2ec384f.jpg",
