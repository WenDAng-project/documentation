le": [{"type": "customvocab:23", "property_id": 563, "property_label": "graffiti style", "is_public": true, "@value": "gothic calligraffiti"}], "write:tag_of": [{"type": "resource:item", "property_id": 551, "property_label": "tag of", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/1145", "value_resource_id": 1145, "value_resource_name": "items", "url": null, "display_title": "Kwanyin Clan", "thumbnail_url": "https://writeproject.dipintra.it/files/square/e4d723651b5cc4033d46652424044496f02af56d.jpg", "thumbnail_title": "Kwanyin Clan", "thumbnail_type": "image/bmp"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/455", "@type": ["o:Item", "write:CalliWritingUnit"], "o:id": 455, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/143", "o:id": 143}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/12", "o:id": 12}, "o:thumbnail": null, "o:title": "CWU 3 Shirupozhu (\u89c2\u97f3, ERICTIN, NAT, YUMI, QUANER)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/ef567746c64623bad5a9fd92d8607e9fd3d37dcd.jpg", "medium": "https://writeproject.dipintra.it/files/medium/ef567746c64623bad5a9fd92d8607e9fd3d37dcd.jpg", "square": "https://writeproject.dipintra.it/files/square/ef567746c64623bad5a9fd92d8607e9fd3d37dcd.jpg"}, "o:created": {"@value": "2023-02-17T14:43:46+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-08T15:56:03+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/456", "o:id": 456}], "o:item_set": [], "o:site": [], "write:attributed-name": [{"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "CWU 3 Shirupozhu (\u89c2\u97f3, ERICTIN, NAT, YUMI, QUANER)", "@language": "en"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This Calli-Writing Unit is the red inscription on the lower left side of the artwork. In this inscription, Chinese characters and Latin letters are juxtaposed, like in other Kwanyin Clan\u2019s works: the two characters GuanYin \u89c2\u97f3 [\u89c0\u97f3] are written vertically in cursive-running style (xingcaoshu \u884c\u8349\u4e66), in the form of a traditional colophon with the signature of the artist (Kwanyin) near the main calligraphy (the four characters shirupozhu); while the four tags of the members of the crew, who were the authors of the artwork (ERICTIN, NAT, YUMI, QUANER), are written horizontally from left to right, as it is in every graffiti piece. Even if in these two lines (vertical and horizontal) the writing systems are different (Chinese characters vs Latin letters), the writing style (of both characters and letters) is identical, so that the two inscriptions seem to be the same thing, merging Chinese and Western traditions into one. This graffiti inscription recalls the use of red seals in traditional Chinese art for three reasons: the red color, the position and the content (signature of the artists). ", "@language": "en"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q2066608", "o:label": "spray painting"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q55439", "o:label": "panel painting"}], "wd:P2079": [{"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q659495", "o:label": "aerosol spray"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@i