ass": {"@id": "https://writeproject.dipintra.it/api/resource_classes/125", "o:id": 125}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/5", "o:id": 5}, "o:thumbnail": null, "o:title": "Sun Ping \u5b59\u5e73", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/b69eb668b23e92f72321b94d598ed5b7c0a8b393.jpg", "medium": "https://writeproject.dipintra.it/files/medium/b69eb668b23e92f72321b94d598ed5b7c0a8b393.jpg", "square": "https://writeproject.dipintra.it/files/square/b69eb668b23e92f72321b94d598ed5b7c0a8b393.jpg"}, "o:created": {"@value": "2023-04-09T07:55:38+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T16:45:01+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/644", "o:id": 644}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1172", "o:id": 1172}], "o:site": [], "wd:P2561": [{"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "Sun Ping \u5b59\u5e73", "@language": "en"}, {"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "\u5b59\u5e73", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "Born in Heilongjiang in 1953, Sun Ping \u5b59\u5e73joined the \u201cChina Artists Association\u201d (Zhongguo meishu jia xiehui\u4e2d\u56fd\u7f8e\u672f\u5bb6\u534f\u4f1a) in 1982 and graduated from the \u201cGuangzhou Academy of Fine Arts\u201d (Zhongguo Guangzhou meishu xueyuan\u4e2d\u56fd\u5e7f\u5dde\u7f8e\u672f\u5b66\u9662) in China in 1987. In 1990 he created the series \u201cTrace\u201d (Henji\u75d5\u8ff9) and in 1991 he created the series \u201cAcupuncture\u201d (Zhenjiu\u9488\u7078). In 1993, he participated in the \u201cTravelling Exhibition of Post-1989 Chinese Contemporary Art\u201d (Hou \u201889 Zhongguo dangdai yishu xunhui zhan \u540e\u201889\u4e2d\u56fd\u5f53\u4ee3\u827a\u672f\u5de1\u56de\u5c55) held in Hong Kong and the United States with the painting \u201cIssuing Stocks\u201d (Faxing gupiao\u53d1\u884c\u80a1\u7968). In 1998, he participated in the \u201c\u201998 Asian Modern Art Exhibition\u201d (\u201998 Yazhou xiandai meishu zhan \u201898\u4e9a\u6d32\u73b0\u4ee3\u7f8e\u672f\u5c55). From 1999 to 2000, he participated in the \u201cCross-Century Rainbow - Gaudy Art Exhibition\u201d (Kua shiji caihong - Yan su yishu zhan \u8de8\u4e16\u7eaa\u5f69\u8679 - \u8273\u4fd7\u827a\u672f\u5c55) in Tianjin, with \u201cMiss China\u201d (Zhongguo xiaojie \u4e2d\u56fd\u5c0f\u59d0). From 1999 to 2002, he founded the magazine \u201cVision 21\u201d (Shijue 21\u89c6\u89c921).", "@language": "en"}], "wd:P21": [{"type": "uri", "property_id": 406, "property_label": "gender", "is_public": true, "@id": "https://www.wikidata.org/entity/Q6581097", "o:label": "male"}], "wd:P27": [{"type": "uri", "property_id": 413, "property_label": "country of citizenship", "is_public": true, "@id": "https://www.wikidata.org/entity/Q148", "o:label": "People's Republic of China"}], "wd:P569": [{"type": "numeric:timestamp", "property_id": 407, "property_label": "date of birth", "is_public": true, "@value": "1953-09-20", "@type": "http://www.w3.org/2001/XMLSchema#date"}], "wd:P19": [{"type": "uri", "property_id": 408, "property_label": "place of birth", "is_public": true, "@id": "https://www.wikidata.org/entity/Q19206", "o:label": "Heilongjiang"}], "wd:P106": [{"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3303330", "o:label": "calligrapher"}, {"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q21477194", "o:label": "contemporary artist"}], "write:academic-artistic-formation": [{"type": "uri", "property_id": 569, "property_label": "academic artistic formation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q106371273", "o:label": "fine arts"}], "write:calligraphic-formation": [{"type": "customvocab:21", "property_id": 497, "property_label": "has calligraphic formation", "is_public": true, "@value": "Academic", "@language": "en"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/645", "@type": ["o:Item", "wdt:Q215627"], "o:id": 645, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/5", "o:id": 5}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/125", "o:id": 125}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/5", "o:id": 5}, "o:thumbnail": null, "o:title": "Jessie Yingying Gong \u9f9a\u9896\u9896", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/c4ca7b4330cac86b345390aec88832ac97a89626.jpg", "medium": "https://writeproject.dipintra.it/files/medium/c4ca7b4330cac86b345390aec88832ac97a89626.jpg", "square": "https://writeproject.dipintra.it/files/square/c4ca7b4330cac86b345390aec88832ac97a89626.jpg"}, "o:created": {"@value": "2023-04-09T14:44:38+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:21:00+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/646", "o:id": 646}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1172", "o:id": 1172}], "o:site": [], "wd:P2561": [{"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "Jessie Yingying Gong \u9f9a\u9896\u9896", "@language": "en"}, {"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "\u9f9a\u9896\u9896", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "Having been born and raised in China (Jiansu) before setting out to study and live in Europe; prompted in Jessie a lasting fascination with the topics of memory, identity, symbols and language. She is currently developing a series of works regarding the form, materialization and development of language. The research into these aspects of language, with a focus on the written forms, leads her to a kind of archaeology of symbolism, that serves to chronicle our history on a multitude of levels.\nJessie studied in London College of Communication and Gerrit Rietveld Academie in Amsterdam. She has completed several artist residencies, and been awarded Young Talent and Artist Project contributions from Mondriaan Fonds. She actively participates in international exhibitions; her work has been to Venice Biennale, Beijing Photo Biennale, MAXXI, Art Rotterdam, W139, etc. Jessie is based in Amsterdam and Shanghai.", "@language": "en"}], "wd:P21": [{"type": "uri", "property_id": 406, "property_label": "gender", "is_public": true, "@id": "https://www.wikidata.org/entity/Q6581072", "o:label": "female"}], "wd:P27": [{"type": "uri", "property_id": 413, "property_label": "country of citizenship", "is_public": true, "@id": "https://www.wikidata.org/entity/Q148", "o:label": "People's Republic of China"}], "wd:P569": [{"type": "numeric:timestamp", "property_id": 407, "property_label": "date of birth", "is_public": true, "@value": "1990-10-14", "@type": "http://www.w3.org/2001/XMLSchema#date"}], "wd:P19": [{"type": "uri", "property_id": 408, "property_label": "place of birth", "is_public": true, "@id": "https://www.wikidata.org/entity/Q16963", "o:label": "Jiangsu"}], "wd:P106": [{"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3391743", "o:label": "visual artist"}, {"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true,