		"write:description": [
			{
				"type": "literal",
				"property_id": 560,
				"property_label": "description",
				"is_public": true,
				"@value": "This poem was written by the calligrapher and painter Huang Yi 黄易（1744-1802） on his painting \"Bamboo in the rain\" (Yu zhu tu 雨竹图). Huang Yi, originally from Qiantang, lived in Shaoxing for a long time. He was one of the \"Eight Schools of Xiling (Xiling ba jia 西泠八家)\". Together with Xi Gang 奚冈 and Wu Lu 吴履, they are called the \"Three Wonders of Western Zhejiang (Zhe xi san miao 浙西三妙)\", and together with Ding Jing 丁敬, Jiang Ren 蒋仁 and Xi Gang 奚冈, they are called the \"Four Famous Masters of Hangzhou Commandery  (Hang jun si jia 杭郡四家)\".",
				"@language": "en"
			}
		],
		"write:trascription-or-translation": [
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "雨洗修篁分外青，萧萧如在过溪亭。\n世间都是无情物，只有秋声最好听。\n",
				"@language": "zh"
			},
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "\"The rain washes the bamboo poles exceptionally clear, and the rustles are as if they are in the pavilion crossing the stream; there are all heartless things in the world, but the sound of autumn is the most beautiful.\" ",
				"@language": "en"
			},
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "Yǔ xǐ xiū huáng fèn wài qīng, xiāoxiāo rú zàiguò xī tíng.\nShìjiān dōu shì wúqíng wù, zhǐyǒu qiūshēng zuì hǎo tīng.",
				"@language": "pny"
			}
		],
