"o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/143", "o:id": 143}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/12", "o:id": 12}, "o:thumbnail": null, "o:title": "CWU The Thirty-Six Stratagems of using ink (\u58a8\u6cd5\u4e09\u5341\u516d\u8ba1)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/51337d6d5bc7f9add2f4a572966051c921d73da2.jpg", "medium": "https://writeproject.dipintra.it/files/medium/51337d6d5bc7f9add2f4a572966051c921d73da2.jpg", "square": "https://writeproject.dipintra.it/files/square/51337d6d5bc7f9add2f4a572966051c921d73da2.jpg"}, "o:created": {"@value": "2023-01-16T12:00:37+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2023-03-14T11:58:05+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/320", "o:id": 320}], "o:item_set": [], "o:site": [], "write:attributed-name": [{"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "CWU The Thirty-Six Stratagems of using ink (\u58a8\u6cd5\u4e09\u5341\u516d\u8ba1)", "@language": "en"}, {"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "Mofa sanshiliuji", "@language": "pny"}, {"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "\u58a8\u6cd5\u4e09\u5341\u516d\u8ba1", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This CWU is part of the artwork \u201cThe Thirty-Six Stratagems of using ink\u201d, composed of 36 squares executed in 2008 by Shao Yan using a new tool: a syringe with a small ink reservoir. \nThis CWU is Shao Yan's first experiment using a syringe of ink instead of a brush, the so called \u201cink shooting\u201d (shemo \u5c04\u58a8) to write lines without meaning. The thick ink, squirting out of the syringe toward the paper, reminds of blood and the artist's life is the force that drives the tool. The entire artistic process is characterized by the force of the syringe, the force of gravity, the speed of the jet and the direction that create numerous ink marks of multiple shapes. Through this early experiment with the syringe, Shao Yan was amazed by the fan-like shapes that the ink, hurled at high speed like a projectile, formed on the paper, and this expedient enabled him to produce different shapes of lines and marks.", "@language": "en"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q127418", "o:label": "ink"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q11472", "o:label": "paper"}], "wd:P2079": [{"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q273318", "o:label": "syringe"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}], "write:background-color": [{"type": "uri", "property_id": 561, "property_label": "background color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23444", "o:label": "white"}], "write:calligraphic-line": [{"type": "customvocab:25", "property_id": 555, "property_label": "calligraphic line", "is_public": true, "@value": "Present"}], "write:significance": [{"type": "customvocab:24", "property_id": 556, "property_label": "significance", "is_public": true, "@value": "Set of lines (meaningless)"}], "wd:P144": [{"type": "resource:item", "property_id": 537, "property_label": "based on", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/318", "value