		"write:description": [
			{
				"type": "literal",
				"property_id": 560,
				"property_label": "description",
				"is_public": true,
				"@value": "\"Yuqian Monk and Green Bamboo House\" (Yu qian seng lu yun xuan 於潜僧绿筠轩)  is a poem by Su Shi 苏轼（1037—1101).",
				"@language": "en"
			}
		],
		"write:trascription-or-translation": [
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "可使食無肉，不可居無竹。\n無肉令人瘦，無竹令人俗。\n人瘦尚可肥，俗士不可醫。\n旁人笑此言，似高還似痴。\n若對此君仍大嚼，世間哪有揚州鶴。",
				"@language": "zh"
			},
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "Kě shǐ shí wú ròu, bùkě jū wú zhú.\nWú ròu lìng rén shòu, wú zhú lìng rén sú.\nRén shòu shàng kě féi, sú shì bùkě yī.\nPángrén xiào cǐ yán, shì gāo hái shì chī.\nRuò duì cǐjūn réng dà jué, shìjiān nǎ yǒu yángzhōu hè.",
				"@language": "pny"
			}
		],
