:owner": {"@id": "https://writeproject.dipintra.it/api/users/5", "o:id": 5}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/125", "o:id": 125}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/5", "o:id": 5}, "o:thumbnail": null, "o:title": "An Daoyi \u5b89\u9053\u58f9", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/9eb394d0dc02bc4a20e6643053c429247bdb1d6f.jpg", "medium": "https://writeproject.dipintra.it/files/medium/9eb394d0dc02bc4a20e6643053c429247bdb1d6f.jpg", "square": "https://writeproject.dipintra.it/files/square/9eb394d0dc02bc4a20e6643053c429247bdb1d6f.jpg"}, "o:created": {"@value": "2023-01-15T15:07:44+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:20:59+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/298", "o:id": 298}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1172", "o:id": 1172}], "o:site": [], "wd:P2561": [{"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "An Daoyi \u5b89\u9053\u58f9", "@language": "en"}, {"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "\u5b89\u9053\u58f9", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "An Daoyi, also known as the monk An Daoyi, was born in Dongping, Shandong. He was a monk and a great calligrapher of the Northern Qi dynasty.", "@language": "en"}], "write:wikidata-resource": [{"type": "uri", "property_id": 567, "property_label": "wikidata resource", "is_public": true, "@id": "https://www.wikidata.org/entity/Q48886885", "o:label": "An Daoyi"}], "wd:P570": [{"type": "numeric:timestamp", "property_id": 409, "property_label": "date of death", "is_public": true, "@value": "0580", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "wd:P19": [{"type": "uri", "property_id": 408, "property_label": "place of birth", "is_public": true, "@id": "https://www.wikidata.org/entity/Q43407", "o:label": "Shandong"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/300", "@type": ["o:Item", "wdt:Q4502142"], "o:id": 300, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/5", "o:id": 5}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/147", "o:id": 147}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/8", "o:id": 8}, "o:thumbnail": null, "o:title": "This Baiju Valley (\u6b64\u767d\u9a79\u8c37)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/368a4eb476bc9ff11bba53a40e57fa38faffb406.jpg", "medium": "https://writeproject.dipintra.it/files/medium/368a4eb476bc9ff11bba53a40e57fa38faffb406.jpg", "square": "https://writeproject.dipintra.it/files/square/368a4eb476bc9ff11bba53a40e57fa38faffb406.jpg"}, "o:created": {"@value": "2023-01-15T18:15:28+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:24:57+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/303", "o:id": 303}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1173", "o:id": 1173}], "o:site": [], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "This Baiju Valley (\u6b64\u767d\u9a79\u8c37)", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Ci Baiju gu", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "\u6b64\u767d\u9a79\u8c37", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "The inscription of this rubbing is entitled \"This Baiju Valley\" (Ci Baiju gu \u6b64\u767d\u9a79\u8c37). According to the data, this inscription was inscribed by Zheng Daozhao \u90d1\u9053\u662d (? - 515) during his tenure as the prefect of Qingzhou. The full text of the inscription is \"Zhongyue xiansheng ying yang zheng daozhao you pan zhii shangu ye ci bai ju gu \u4e2d\u5cb3\u5148\u751f\u8367\u9633\u90d1\u9053\u662d\u6e38\u69c3\u4e4b\u5c71\u8c37\u4e5f\u6b64\u767d\u9a79\u8c37\" (\"The Valley where Mr. Zhongyue Yingyang Zheng Daozhao visited Pan is also Baiju Valley\") and \"This Baiju Valley\" has a total of 19 characters. The entire inscription is one foot square in diameter. It is a precious treasure of calligraphy art in Weifang.\nThe technique of rubbing consists in applying a sheet of paper with longer and more resisting fibers to the rocky surface of a cliff, stele or even small seal stone, to then ink the sheet, and take a life-size copy of the rubbed surface. An inversion in the perception of relief and shade takes place when intaglio lines become white, and relief parts, black. Obviously, while deep intaglio is totally white and high relief, black, there is a whole realm of in-betweenness where the middle grounds, low relief and smooth concavities can only be expressed in shades of grey. Rubbing is thus extremely interpretative and is a form of indirect perception, comparable to photography.\n", "@language": "en"}], "wd:P170": [{"type": "resource:item", "property_id": 429, "property_label": "creator", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/301", "value_resource_id": 301, "value_resource_name": "items", "url": null, "display_title": "Zheng Daozhao \u90d1\u9053\u662d", "thumbnail_url": "https://writeproject.dipintra.it/files/square/7afbd0abb9e236ebe99010e13911cfd9a0ec0c73.jpg", "thumbnail_title": "Zheng Daozhao", "thumbnail_type": "image/jpeg"}], "write:artwork-type": [{"type": "uri", "property_id": 505, "property_label": "artwork type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q1640824", "o:label": "inscription"}, {"type": "uri", "property_id": 505, "property_label": "artwork type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q7375860", "o:label": "rubbing"}], "wd:P1071": [{"type": "uri", "property_id": 430, "property_label": "location of creation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q13958964", "o:label": "Baiju, Jiangsu"}], "wd:P571": [{"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "0513", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}, {"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "0515", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "wd:P2049": [{"type": "literal", "property_id": 432, "property_label": "width", "is_public": true, "@value": "92 cm "}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q11472", "o:label": "paper"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q127418", "o:label": "ink"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q8063", "o:label": "rock"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}, {"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23444", "o:label": "white"}, {"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.w