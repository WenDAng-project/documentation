/api-context", "@id": "https://writeproject.dipintra.it/api/items/844", "@type": ["o:Item", "wdt:Q215627"], "o:id": 844, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/5", "o:id": 5}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/125", "o:id": 125}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/5", "o:id": 5}, "o:thumbnail": null, "o:title": "Sainkho Namtchylak", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/54a91cb4c01efc1dd24f012c4d796805de3d6268.jpg", "medium": "https://writeproject.dipintra.it/files/medium/54a91cb4c01efc1dd24f012c4d796805de3d6268.jpg", "square": "https://writeproject.dipintra.it/files/square/54a91cb4c01efc1dd24f012c4d796805de3d6268.jpg"}, "o:created": {"@value": "2023-07-22T12:09:57+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:21:00+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/845", "o:id": 845}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1172", "o:id": 1172}], "o:site": [], "wd:P2561": [{"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "Sainkho Namtchylak", "@language": "en"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "Sainkho Namtchylak is a singer originally from Tuva, an autonomous republic in the Russian Federation just north of Mongolia. She is known for her Tuvan throat singing[1] or Kh\u00f6\u00f6mei.Namtchylak is an experimental singer, born in 1957 in a secluded village in the south of Tuva. She is proficient in overtone singing; her music encompasses avant-jazz, electronica, modern composition and Tuvan influences. In Tuva, numerous cultural influences collide: the Turkic roots and culture it shares with Central Asian states, the Xinjiang Uyghur Autonomous Region, Bashkortostan and Tatarstan; the strong Mongolic cultural influence and traditions it shares with Mongolia, Inner Mongolia, Buryatia and Kalmykia; the cultural influences from the various Siberian nomadic ethnic groups such as Samoyeds, Yeniseians, Evenks and from the Russian Old Believers, the migrant and resettled populations from Ukraine, Tatarstan and other minority groups west of the Urals. All of these, to extents, impact on Namtchylak's voice, although the Siberian influences dominate: her thesis produced while studying voice, first at the University of Kyzyl, then in the Gnesins Institute in Moscow during the 1980s focussed on Lamaistic and cult musics of minority groups across Siberia, and her music frequently shows tendencies towards Tungus-style imitative singing.", "@language": "en"}], "wd:P21": [{"type": "uri", "property_id": 406, "property_label": "gender", "is_public": true, "@id": "https://www.wikidata.org/entity/Q6581072", "o:label": "female"}], "wd:P27": [{"type": "uri", "property_id": 413, "property_label": "country of citizenship", "is_public": true, "@id": "https://www.wikidata.org/entity/Q159", "o:label": "Russia"}], "wd:P569": [{"type": "numeric:timestamp", "property_id": 407, "property_label": "date of birth", "is_public": true, "@value": "1957", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "wd:P19": [{"type": "uri", "property_id": 408, "property_label": "place of birth", "is_public": true, "@id": "https://www.wikidata.org/entity/Q960", "o:label": "Tuva"}], "wd:P106": [{"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q177220", "o:label": "singer"}], "wd:P103": [{"type": "uri", "property_id": 583, "property_label": "native language", "is_public": true, "@id": "https://www.wikidata.org/entity/Q34119", "o:label": "Tuvan"}], "wd:P1412": [{"type": "uri", "property_id": 412, "property_label": 