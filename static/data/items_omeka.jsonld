lay_urls": {"large": "https://writeproject.dipintra.it/files/large/024152f8002c070a635bc24d75e6dea397a4289c.jpg", "medium": "https://writeproject.dipintra.it/files/medium/024152f8002c070a635bc24d75e6dea397a4289c.jpg", "square": "https://writeproject.dipintra.it/files/square/024152f8002c070a635bc24d75e6dea397a4289c.jpg"}, "o:created": {"@value": "2022-11-12T10:21:42+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2023-03-13T17:31:10+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/273", "o:id": 273}], "o:item_set": [], "o:site": [], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Zhang Qiang's Traceology A/B Model  (\u5f20\u5f3a\u8e2a\u8ff9\u5b66\u62a5\u544a A/B \u6a21\u578b)", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Zhang Qiang zongji xue baogao A/B moxing", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": " \u5f20\u5f3a\u8e2a\u8ff9\u5b66\u62a5\u544a A/B \u6a21\u578b", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This traceology series lasts from 1990 to 2000. In this series, Zhang Qiang established cooperative relationships with 100 different women.\nZhang Qiang realizes calligraphy, and each woman operates according to her own will, including the movement range of the paper, the size of the brush and the intensity of the ink used, and even when the performances will stop.  Zhang Qiang turned his side or back when writing, and no longer looked at the paper he was writing on. The content of the writing is realized extemporaneously. He sometimes writes recognizable Chinese characters and other times the characters are deconstructed due to the movement of the female counterpart. Since Zhang Qiang no longer looks at the paper, the woman can decide all the performance results. \nEach  collaboration includes 8 works on paper and sometimes also on plaster's geometric shapes. During the performance, other factors also come into play, such as the personal situation of the women, a dialogue with them, piped music, documentary photos of the performance process, and usually some spectators. However, performances are recorded only on rare occasions.\nVariable system of the \"Zhang Qiang's Traceology Report A/B Model\":\n(1) After B30, some cooperation began to be presented in the form of questions and answers.\n(2) From B21, performances began to involve women from ethnic minorities. \n(3) From B24, performances began to involve foreign women.\n(4) From B50, calligraphy began to appear on solid geometric shapes made of plaster.\n(5) From B55, calligraphy began to be on swimsuit and bodies of the cooperating women.\n(6) The calligraphy of the performance B94 is realized on clothes made of rice paper.", "@language": "en"}], "wd:P170": [{"type": "resource:item", "property_id": 429, "property_label": "creator", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/100", "value_resource_id": 100, "value_resource_name": "items", "url": null, "display_title": "Zhang Qiang \u5f20\u5f3a", "thumbnail_url": "https://writeproject.dipintra.it/files/square/29b13d7785f963531a0ff4b34f9572b13213e01b.jpg", "thumbnail_title": "Zhang Qiang", "thumbnail_type": "image/jpeg"}], "wd:P1071": [{"type": "uri", "property_id": 430, "property_label": "location of creation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q148", "o:label": "People's Republic of China"}], "wd:P582": [{"type": "numeric:timestamp", "property_id": 452, "property_label": "end time", "is_public": true, "@value": "1990", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "wd:P580": [{"type": "numeric:timestamp", "property_id": 453, "property_label": "start time", "is_public": true, "@value": "2000", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/276", "@type": ["o:Item", "wdt:Q4502142"], "o:id": 276, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/3", "o:id": 3}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/147", "o:id": 147}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/8", "o:id": 8}, "o:thumbnail": null, "o:title": "Bright Snow (\u6674\u96ea\uff09", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/1eaaf5ebe528790b201291ea97955f4e7fad3bf4.jpg", "medium": "https://writeproject.dipintra.it/files/medium/1eaaf5ebe528790b201291ea97955f4e7fad3bf4.jpg", "square": "https://writeproject.dipintra.it/files/square/1eaaf5ebe528790b201291ea97955f4e7fad3bf4.jpg"}, "o:created": {"@value": "2022-12-14T10:00:27+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:24:57+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/285", "o:id": 285}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1173", "o:id": 1173}], "o:site": [], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Bright Snow (\u6674\u96ea\uff09", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Qing xue", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "\u6674\u96ea", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "\u201cBright snow\u201d, executed in 1985 by Shao Yan, is an artwork composed by two characters and a lower-left little inscription. The wo characters, qing  \u6674 (\u201cbright\u201d), whose edges look like carved wood signs, and xue \u96ea (\u201csnow\u201d), fat and thick with a firm central part, represent a surprising contrast just like the sunlight that spreads with roar over the silent snow. For both characters, Shao Yan adopts the \u201cflying white\u201d (feibai\u98de\u767d) technique, which means using the half-dry brush, in order to represent the wild landscape of Northern China struck by violent snowstorms. The little inscription is written with brushstrokes full of ink and it has an angular shape, balancing the entire calligraphy. This work is one of the first experimental works by Shao Yan: as the artist was influenced by the \u201cfew characters\u201d Japanese calligraphic movement, the structure of each character embodies the meaning of the character itself.", "@language": "en"}], "wd:P170": [{"type": "resource:item", "property_id": 429, "property_label": "creator", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/277", "value_resource_id": 277, "value_resource_name": "items", "url": null, "display_title": "Shao Yan \u90b5\u5ca9", "thumbnail_url": "https://writeproject.dipintra.it/files/square/9ebee27d8d6c8a61ab743b5fb650741cd7400fb5.jpg", "thumbnail_title": "Shao Yan", "thumbnail_type": "image/jpeg"}], "write:artwork-type": [{"type": "uri", "property_id": 505, "property_label": "artwork type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q12681", "o:label": "calligraphy"}], "wd:P1071": [{"type": "uri", "property_id": 430, "property_label": "location of creation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q43407", "o:label": "Shandong"}], "wd:P571": [{"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "1985", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "wd:P2049": [{"type":