1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Mit diesen H\u00e4nden (\u7528\u8fd9\u53cc\u624b)", "@language": "de"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "With These Hands", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Yong zhe shuang shou", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "\u7528\u8fd9\u53cc\u624b", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "The artwork \"Mit diesen H\u00e4nden\" (\u7528\u8fd9\u53cc\u624b) was created by Huang Miaozi in 1992 while he was a visiting fellows at the Heinrich B\u00f6ll Institute in Koln in Cologne, Germany. Together with the Chinese calligrapher and artist Gu Gan, friend of Huang Miaozi, he organized an exhibition of their works, held at the Museum f\u00fcr Ostasiatische Kunst in Cologne at the end of 1992. This exhibition was the first major modern Chinese calligraphy exhibition held in Europe or the United States and Huang's \"Mit diesen H\u00e4nden\" represents the most memorable piece from this exhibition. The title of this artwork, inspired by Heinrich B\u00f6ll's poem \"Mit diesen H\u00e4nden\", gave also the name to the exhibition. The artwork reminded Huang Miaozi of the hard times faced while living in China. On the purple print of an open hand, he wrote part of the poem in the archaic Seal Script of the Shang-Zhou period. According to Barrass (2002 p. 178), this is the only example of his works that Huang refused to sell.\n", "@language": "en"}], "wd:P170": [{"type": "resource:item", "property_id": 429, "property_label": "creator", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/744", "value_resource_id": 744, "value_resource_name":