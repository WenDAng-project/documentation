		"write:description": [
			{
				"type": "literal",
				"property_id": 560,
				"property_label": "description",
				"is_public": true,
				"@value": "(Zhu shi 竹石) is a four-line poem with seven-character quatrain (七言绝句) created by Zheng Xie 郑燮（also known as Zheng Banqiao ）（1693—1765）, a calligrapher and painter of the Qing Dynasty.\nThis is a poem on a paiting (题画诗)  praising the inner spirit of bamboo rooted in broken rocks, which fully expresses Jinzhu's tenacious vitality and resolute character.\n",
				"@language": "en"
			}
		],
		"write:trascription-or-translation": [
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "咬定青山不放松，立根原在破岩中。\n千磨万击还坚劲，任尔东西南北风。",
				"@language": "zh"
			},
			{
				"type": "literal",
				"property_id": 570,
				"property_label": "transcription and translation",
				"is_public": true,
				"@value": "Yǎodìng qīngshān bù fàngsōng, lìgēn yuán zài pò yán zhōng.\nQiān mó wàn jī hái jiān jìn, rèn ěr dōngxī nánběi fēng.",
				"@language": "pny"
			}
		],
