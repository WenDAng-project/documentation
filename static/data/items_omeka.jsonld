": [{"@id": "https://writeproject.dipintra.it/api/media/149", "o:id": 149}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1173", "o:id": 1173}], "o:site": [{"@id": "https://writeproject.dipintra.it/api/sites/4", "o:id": 4}], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "2018 From Ink to Apparel III_TSAI Yi-Fen", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "2018 Du yi III - Cai Yifen", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "2018 \u8bfb\u8863III \u2014\u2014\u8521\u5b9c\u82ac", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This collection by Tsai Yi-fen is part of the exhibition \"2018 From Ink to Apparel III\" that was held in Taipei. The purpose of this exhibition is to promote Chinese culture in Taiwan through calligraphy art. Curator Tong Yang-Tze invites diverse and talented fashion designers of Chinese ethnicity to participate, create, and accumulate design context inspired by the calligraphy lines. Besides Tsai Yi-fen, other five desigers took part in the exhibition: two from Taiwan ( Chou Yu-Ying, Chen Shao-Yen), two from Beijing (Li Ying-Jun, Zhou You) and one from UK (Pan Bernice).The exhibition theme is \u201cw\u00fa\u201d \u7121 (emptiness). Tong Yang-tze wrote fourteen fonts of this character which are variations of the Cursive Script, Running Script, Regular Script, Clerical Script, and Seal Script. The forms of these characters are distinct, but designers must interpret them visually to form structures, materials, and prints from the fluidity of calligraphy lines and the spaces amongst them.\nThis collection by Tsai Yi-fen is composed of five dresses. It combines lacework and beaded embroideries to create unique textures and tailoring, displaying the space between the real and the void.", "@language": "en"}], "write:product-type": [{"type": "uri", "property_id": 506, "property_label": "product type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q29583", "o:label": "fashion design"}, {"type": "uri", "property_id": 506, "property_label": "product type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q200539", "o:label": "dress"}, {"type": "uri", "property_id": 506, "property_label": "product type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q11460", "o:label": "clothing"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q231250", "o:label": "lace"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q18281", "o:label": "embroidery"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q1053956", "o:label": "bead"}], "wd:P180": [{"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q546054", "o:label": "\u015a\u016bnyat\u0101"}, {"type": "literal", "property_id": 420, "property_label": "depicts", "is_public": true, "@value": "worldliness"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}, {"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23444", "o:label": "white"}], "wd:P571": [{"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "2018", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "wd:P1071": [{"type": "uri", "property_id": 430, "property_label": "location of