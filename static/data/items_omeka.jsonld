, "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1173", "o:id": 1173}], "o:site": [{"@id": "https://writeproject.dipintra.it/api/sites/4", "o:id": 4}], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Flute Melodies,  Flowing Water, Writing Calligraphy (\u7bab\u58f0 \u00b7  \u6d41 \u6c34 \u00b7 \u4e66\u5199)", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Xiao sheng, liu shui, shuxie", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "\u7bab\u58f0 \u00b7  \u6d41 \u6c34 \u00b7 \u4e66\u5199", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "Zhu Qingsheng and Zhang Weiliang had a small dwelling near a stream that flows on Mount Huairou. Wearing a cloak of straw (suoyi \u84d1\u8863) and a bamboo hat (douli \u6597\u7b20) and bringing with them paintings, calligraphies, and musical instruments, they wanted to look like people returning to their native village. Then, with the aid of a bamboo flute (xiao \u7bab), calligraphy and a silk scroll, they reproduced a banquet of literati: Zhang Weiliang played the flute, which does not sing melodies but reproduced the sound of water flowing and breaking on the boulders; in the meanwhile, Zhu Qingsheng wrote a calligraphy poem on the silk scroll suspended on the stream, that soaked it. Only the parts of the scroll that lay on the rock kept traces of ink; on those that instead sank in the stream, Zhu Qingsheng wrote directly in the water that washed away the ink. As a result, the \u201cabsorbed\u201d characters had been taken away and only the soaked silk scroll remains."}], "write:performance-art-type": [{"type": "customvocab:15", "property_id": 507, "property_label": "performance art type", "is_public": true, "@id":