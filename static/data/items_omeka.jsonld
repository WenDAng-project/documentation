"o:site": [], "write:attributed-name": [{"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "CWU Sea (\u6d77)\n", "@language": "en"}, {"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "Hai", "@language": "pny"}, {"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "\u6d77", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This CWU is part of the artwork \u201cSea\u201d (\u6d77), executed in 2001 by Shao Yan. The artist introduces a new technique consisting of the repeated execution of strokes on the paper which will later be washed in water. After the washing process, the character's structure shines clearly on the paper, with a large amount of ink and various intense gradiations. Shao Yan pays particular attention to the spacial order and the composition of the character but, introducing the element of water, he adds the concept of time to the construction of calligraphic space. He thus emphasises the instantaneous and unrepeatable nature of artistic creation.", "@language": "en"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q127418", "o:label": "ink"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q283", "o:label": "water"}], "wd:P2079": [{"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q614467", "o:label": "brush"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}], "write:background-color": [{"type": "uri", "property_id": 56