, "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1173", "o:id": 1173}], "o:site": [], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Army (\u519b)", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Jun", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "\u519b", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "\"Army\" (Jun \u519b) is an artwork executed in 2003 by Shao Yan. Influenced by Western analythical cubism, the artist modifies the original structure of the characters preventing their identification, denies the concept of classical perspective and destroys the calligraphic space, breaking it up and reorganising it into simple geometric shapes such as triangles and polygons.\nIn \u201cJun\u201d the geometric structure is concentrated on the central line while the outermost part, towards the edge, is airier and more open. Shao Yan thus wants to emphasise the layering and light effect produced by the geometric cut. The material structure is dominated by the geometric appearance of the lines that gather into inclined angular planes, and the straight lines and oblique planes suggest the changing position of the Chinese character structure. Shao Yan executed the work using ink 'washed' with water, which, unlike Western oil painting, softens the geometric deconstruction of the characters structure.", "@language": "en"}], "wd:P170": [{"type": "resource:item", "property_id": 429, "property_label": "creator", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/277", "value_resource_id": 277, "value_resource_name": "items", "url": null, "display_title": "Shao Yan \u90b5\u5ca9", "thumbnail_url": "https://writeproject.dipintra.it/files/squ