on"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q1760011", "o:label": "prosperity"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q216774", "o:label": "destiny"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/100", "@type": ["o:Item", "wdt:Q215627"], "o:id": 100, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/5", "o:id": 5}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/125", "o:id": 125}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/5", "o:id": 5}, "o:thumbnail": null, "o:title": "Zhang Qiang \u5f20\u5f3a", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/29b13d7785f963531a0ff4b34f9572b13213e01b.jpg", "medium": "https://writeproject.dipintra.it/files/medium/29b13d7785f963531a0ff4b34f9572b13213e01b.jpg", "square": "https://writeproject.dipintra.it/files/square/29b13d7785f963531a0ff4b34f9572b13213e01b.jpg"}, "o:created": {"@value": "2021-10-09T13:25:25+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T11:20:59+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/573", "o:id": 573}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1172", "o:id": 1172}], "o:site": [{"@id": "https://writeproject.dipintra.it/api/sites/4", "o:id": 4}], "wd:P2561": [{"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "Zhang Qiang \u5f20\u5f3a", "@language": "en"}, {"type": "literal", "property_id": 405, "property_label": "name", "is_public": true, "@value": "\u5f20\u5f3a", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "Zhang Qiang is an artistic revolutionary with strongly conservative credentials. He has a deep understanding of traditional Chinese painting and calligraphy, and a thorough knowledge of the aesthetic theories that underpin them. He is also a leading theorist of the Avant-Garde movement in calligraphy and the founder of \"traceology\", an entirely new genre that is centred on enlisting the aid of a female partner in creating each work.\nHe was born in 1962 and brought up in Shandong. His father was the head of a huge agricultural commune comprising some 30,000 people and his mother was in charge of its 'women's affairs'. He went to the Taian Teacher's Training College, near Mount Tai, and three years later, while teaching at the middle school in his home town of Feicheng, he began to take a serious interest in Chinese art. The Shandong Arts Institute in Jinan appointed him to its staff as a lecturer and in 1993 he was made director of the newly created Office of Fine Arts Studies. He believed that including meaningful text in calligraphy would provide great scope for developing it as a modern art form. In 1993 he started experimenting with the help of female partners. He started with a student of Chinese painting who wrote Avant-Garde poetry characterized by disjunctions of time and place. As she read her poems aloud, Zhang Qian would try to write down the words in an abstract way. One night he got the woman to move his paper while he was writing down on it the words she was reciting. Then a flash of inspiration made him turn his head away so that he could not see his partner while he was writing, although she could still see his hand moving the brush. This time the results amazed him. All of the sudden, he had succeeding in freeing himself from all the restrictions imposed by the conventional composition of characters. In 1995 he started working with other creative young women. The sentences he wrote out were usually fairly mundane thoughts that happened to be running through his mind, rather than the words of the great Chinese poets or any of the poems he himself writes in classical style. Zhang Qian has described and analysed the whole traceology project in a book entitled \"Trace Aesthetics: Cultural Cross-overs through Art\" (2002). In it he says that during the project he felt as if he was using his brush to construct a building while his partner was exerting an elemental force, like an earthquake, to destroy the whole structure. For him, the shattered traces recall the moods of the women with whom he worked. Others, however, feel that they are like a Chinese version of the Rorschach ink-blot tests, with viewers projecting their own preoccupations on to what they see before them. In 1998 he set about creating a different form of traceology. Having already turned his calligraphy from a scholarly artistic pursuit into an exploration of the use of space in two dimensions, he wanted to experiment with more independent visual forms in three dimensions. He focused on the two most basic three-dimensional forms: the cube and the sphere. He believes that this forms lie at the heart of Western art and so can act as a bridge linking East and West. His installation works consist of long strips of paper on to which his female partner places plastic balls and cubes after he has painted them. Artist and assistant work on one section of the paper before unfolding the next. The woman decides whether she will lay the objects on the paper one by one as each becomes ready, or wait and position them all at once. Women are even more central to Zhang Qiang's art when their own bodies become the medium on which he paints. He was worked in this way with young Chinese women wearing black one-piece swimsuits, in order to create harmony between their black costumes, their black hair and the ink he is applying to their exposed flesh. He has also done a series of works featuring women in white.\nZhang Qiang has adopted a brave artistic approach to stimulating debate about perceptions of calligraphy and their cultural implications for Chinese society. One of his great strengths in doing so is that no one can accuse him of simply waving his brush about like a child. He is a scholar and a professor, widely respected for his knowledge of the history and theory of both Chinese and Western Art.", "@language": "en"}], "wd:P21": [{"type": "uri", "property_id": 406, "property_label": "gender", "is_public": true, "@id": "https://www.wikidata.org/entity/Q6581097", "o:label": "male"}], "wd:P27": [{"type": "uri", "property_id": 413, "property_label": "country of citizenship", "is_public": true, "@id": "https://www.wikidata.org/entity/Q148", "o:label": "People's Republic of China"}], "wd:P569": [{"type": "numeric:timestamp", "property_id": 407, "property_label": "date of birth", "is_public": true, "@value": "1962-01-29", "@type": "http://www.w3.org/2001/XMLSchema#date"}], "wd:P19": [{"type": "uri", "property_id": 408, "property_label": "place of birth", "is_public": true, "@id": "https://www.wikidata.org/entity/Q1198379", "o:label": "Feicheng"}], "wd:P106": [{"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3303330", "o:label": "calligrapher"}, {"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3391743", "o:label": "visual artist"}, {"type": "uri", "property_id": 553, "property_label": "occupation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q15977927", "o:label": "art educator"}], "write:academic-artistic-formation": [{"type": "uri", "property_id": 569, "property_label": "academic artistic formation", "is_public": true, "@id": "https://www.wikidata.org/entity/Q106371273", "o:label": "fine arts"}], "write:calligraphic-formation": [{"type": "customvocab:21", "property_id": 