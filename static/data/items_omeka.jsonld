o:owner": {"@id": "https://writeproject.dipintra.it/api/users/5", "o:id": 5}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/143", "o:id": 143}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/12", "o:id": 12}, "o:thumbnail": null, "o:title": "CWU This Baiju Valley (\u6b64\u767d\u9a79\u8c37)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/a527bc0e25444d946c8ec2c8c1c220c6bc2fd0f3.jpg", "medium": "https://writeproject.dipintra.it/files/medium/a527bc0e25444d946c8ec2c8c1c220c6bc2fd0f3.jpg", "square": "https://writeproject.dipintra.it/files/square/a527bc0e25444d946c8ec2c8c1c220c6bc2fd0f3.jpg"}, "o:created": {"@value": "2023-03-06T09:09:07+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-11T17:55:13+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/533", "o:id": 533}], "o:item_set": [], "o:site": [], "write:attributed-name": [{"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "CWU This Baiju Valley (\u6b64\u767d\u9a79\u8c37)", "@language": "en"}, {"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "Ci bai ju gu", "@language": "pny"}, {"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "\u6b64\u767d\u9a79\u8c37", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This CWU is part of the inscription entitled \"This Baiju Valley\" (Ci Baiju gu \u6b64\u767d\u9a79\u8c37). According to the data, this inscription was inscribed by Zheng Daozhao \u90d1\u9053\u662d (? - 515) during his tenure as the prefect of Qingzhou. The full text of the inscription is \"Zhongyue xiansheng ying yang zheng daozhao you pan zhii shangu ye ci bai ju gu \u4e2d\u5cb3\u5148\u751f\u8367\u9633\u90d1\u9053\u662d\u6e38\u69c3\u4e4b\u5c71\u8c37\u4e5f\u6b64\u767d\u9a79\u8c37\" (\"The valley where Mr. Zhongyue Yingyang Zheng Daozhao visited Pan is also Baiju Valley\") and \"This Baiju Valley\" has a total of 19 characters. The entire inscription is one foot square in diameter. It is a precious treasure of calligraphy art in Weifang.\nThe technique of rubbing consists in applying a sheet of paper with longer and more resisting fibers to the rocky surface of a cliff, stele or even small seal stone, to then ink the sheet, and take a life-size copy of the rubbed surface. An inversion in the perception of relief and shade takes place when intaglio lines become white, and relief parts, black. Obviously, while deep intaglio is totally white and high relief, black, there is a whole realm of in-betweenness where the middle grounds, low relief and smooth concavities can only be expressed in shades of grey. Rubbing is thus extremely interpretative and is a form of indirect perception, comparable to photography.", "@language": "en"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q127418", "o:label": "ink"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q11472", "o:label": "paper"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q8063", "o:label": "rock"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23444", "o:label": "white"}], "write:background-color": [{"type": "uri", "property_id": 561, "property_label": "background color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}, {"type": "uri", "property