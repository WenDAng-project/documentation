: "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/1141", "@type": ["o:Item", "write:CalliWritingUnit"], "o:id": 1141, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/8", "o:id": 8}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/143", "o:id": 143}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/12", "o:id": 12}, "o:thumbnail": null, "o:title": "CWU Not Too Late (\u4e0d\u592a\u665a)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/c8dc93dd352eb6bc55eee689afbcf739f6e56cf2.jpg", "medium": "https://writeproject.dipintra.it/files/medium/c8dc93dd352eb6bc55eee689afbcf739f6e56cf2.jpg", "square": "https://writeproject.dipintra.it/files/square/c8dc93dd352eb6bc55eee689afbcf739f6e56cf2.jpg"}, "o:created": {"@value": "2024-02-26T22:10:49+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-13T08:53:54+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/1142", "o:id": 1142}], "o:item_set": [], "o:site": [], "write:attributed-name": [{"type": "literal", "property_id": 559, "property_label": "attributed name", "is_public": true, "@value": "CWU Not Too Late (\u4e0d\u592a\u665a)", "@language": "en"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "This CWU is part of the installation  Not too late (\u4e0d\u592a\u665a) realized by Feng Mengbo in 2010. \nFeng Mengbo's personal modification (MOD) of the calligraphy consists of slowing down the speed by ten times compared to the original, in order to make the trajectory of movements visible on the screen. These movements slowly transform into characters, strokes, and calligraphic gestures, giving life to a three-dimensional, interactive, abstract, and dynamic calligraphic work.", "@language": "en"}], "wd:P2079": [{"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q7917855", "o:label": "vectorization"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}, {"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q943", "o:label": "yellow"}, {"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q428124", "o:label": "violet"}], "write:background-color": [{"type": "uri", "property_id": 561, "property_label": "background color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23444", "o:label": "white"}], "write:calligraphic-line": [{"type": "customvocab:25", "property_id": 555, "property_label": "calligraphic line", "is_public": true, "@value": "Present"}], "write:big-characters-calligraphy": [{"type": "customvocab:31", "property_id": 584, "property_label": "big characters calligraphy", "is_public": true, "@value": "Not present"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/1143", "@type": ["o:Item", "wdt:Q4502142"], "o:id": 1143, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/8", "o:id": 8}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/147", "o:id": 147}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/8", "o:id": 8}, "o:thumbnail": null, "o:title": "Not Too Late (\u4e0d\u592a\u665a)", "thumbnail_display_urls": {"large": null, "medium": null, "square": null}, "o:created": {"@value": "2024-02-26T23:09:51+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-13T08:53:29+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1173", "o:id": 1173}], "o:site": [], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Not Too Late (\u4e0d\u592a\u665a)", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Bu tai wan", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "\u4e0d\u592a\u665a", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "The song Not Too Late (\u4e0d\u592a\u665a) was created by Hua Dong of the New Pants (Xin kuzi yuedui \u65b0\u88e4\u5b50\u4e50\u961f), a Chinese indie rock band.", "@language": "en"}], "write:artwork-type": [{"type": "uri", "property_id": 505, "property_label": "artwork type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q7366", "o:label": "song"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/1144", "@type": ["o:Item", "wdt:Q1400264"], "o:id": 1144, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/148", "o:id": 148}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/22", "o:id": 22}, "o:thumbnail": null, "o:title": "OOPS crew", "thumbnail_display_urls": {"large": null, "medium": null, "square": null}, "o:created": {"@value": "2024-02-29T09:50:44+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-02-29T09:59:21+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [], "o:item_set": [], "o:site": [], "dcterms:title": [{"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "OOPS crew", "@language": "en"}], "dcterms:description": [{"type": "literal", "property_id": 4, "property_label": "Description", "is_public": true, "@value": "The crew was founded on a blog in 2007 by Jin Ye (Hu(r)ri, Read or Hali), Reign (or Lame), Snow and Tin.G. The birth of the OOPS crew on a blog demonstrates the importance of the Internet for Chinese writers, as the four members did not meet directly on the street. Actually, the crew has hosted other members over the years, including two European artists: Moon, Aekone (a.k.a. Aek, from Yangzhou), Redim, Kite (from Guangxi); and Diase (Italian writer) and Storm (French writer). This highlights how there are many foreign writers operating in China who actively collaborate with Chinese crews and writers, contaminating both themselves and each other.\nThe OOPS crew is the most famous and influential crew in Shanghai. It combines the Western graffiti art movement with the Chinese artistic and cultural tradition, using characters, calligraphy and ancient Chinese symbols, and represents the cosmopolitan / transcultural and transnational trends in Shanghai. In terms of style, the OOPS crew\u2019s pieces refer to the Euro-American tradition, using, specifically, wildstyle with 3D effects, often enriched by a background or figurative elements from the Chinese painting tradition.\n\"The crew cultivates varying perceptions and self-identities in their work, but they are as a whole known for their elaborate pieces combining alphabets, Chinese writing and characters. Currently, many of the local members are occupied by their everyday work and seldom have time to paint\" (Valjakka 2016, 363).\nOne of the most important members of the crew is Tin.G, the only female writer of the crew. She is one of the most acclaimed female writers in the country, who represents the new post-graffiti trend through the adhesion to hip hop culture, visual propaganda of her stickers, evolution of her puppets in open-air illustrations. While not using characters, Tin.G includes elements of Chinese culture in her pieces.", "@language": "en"}], "write:artistic-collective-type": [{"type": "customvocab:32", "property_id": 585, "property_label": "artistic collective type", "is_public": true, "@id": "https://www.wikidata.org/wiki/Q11964195", "o:label": "Crew"}], "wd:P276": [{"type": "uri", "property_id": 417, "property_label": "location", "is_public": true, "@id": "https://www.wikidata.org/entity/Q8686", "o:label": "Shanghai"}, {"type": "uri", "property_id": 417, "property_label": "location", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23817", "o:label": "50 Moganshan Road"}], "wd:P571": [{"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "2007", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "wd:P856": [{"type": "uri", "property_id": 415, "property_label": "website link", "is_public": true, "@id": "https://flickr.com/photos/oops_graffiti_shanghai/albums/72157621964636716/", "o:label": "Oops crew Flickr"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/1145", "@type": ["o:Item", "wdt:Q1400264"], "o:id": 1145, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/148", "o:id": 148}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/22", "o:id": 22}, "o:thumbnail": null, "o:title": "Kwanyin Clan", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/e4d723651b5cc4033d46652424044496f02af56d.jpg", "medium": "https://writeproject.dipintra.it/files/medium/e4d723651b5cc4033d46652424044496f02af56d.jpg", "square": "https://writeproject.dipintra.it/files/square/e4d723651b5cc4033d46652424044496f02af56d.jpg"}, "o:created": {"@value": "2024-02-29T10:17:56+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-25T11:51:32+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/1162", "o:id": 1162}], "o:item_set": [], "o:site": [], "dcterms:title": [{"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": " Kwanyin Clan", "@language": "en"}, {"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "Guanyin", "@language": "pny"}, {"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "\u89c2\u97f3", "@language": "zh"}], "dcterms:description": [{"type": "literal", "property_id": 4, "property_label": "Description", "is_public": true, "@value": "This crew was founded in June 2006 in Beijing (798 Art District) by EricTin (a.k.a. TIN), YUMI, QUAN e JEV (a.k.a. JER). In 2007 the crew was composed of 6 members : NAT(UO), AP(ART), KENO, VIGA, JAK e SCAV (a.k.a. SCAR). In 2012 it was composed of 12 members: Daiyan (Quanr), Liu Zheng (Guantou, EricTIN), Song Tongshu (YUMI), Zheng Xuezhi (APART), Jianshu (Jer), Lu Daning (NAT), Xu Yan (SCAR), Lu Jiayin (Joey), Tang Mi (KENO), Xu Ou (Pluto), Xue Wenhao (VIGA) e Jitou (CTN). From 2020 the crew is composed of 4 members: EricTin, VIGA, JJJ e SHALA.\nThe most active period of the crew lasted until 2010 when most of the crew members, who were art academies students, graduated from university and \"the competitive everyday life in Beijing has forced the members to focus on their daily jobs in order to support their families\" (Valiakka 2016, 361). From 2006 to 2009, the Kwanyin Clan mainly worked in Beijing, even if the crew also created some pieces in Langfang (Hubei), Chongqing, Tianjin, Guilin, Wuhan, and Shenzhen. Then, from 2009, each member of the crew started to work more individually: Jer, for example, has been mainly active in Wuhan, Keno Tang has focused on the embellishment of the hutong in Beijing (especially from 2011), and EricTin has concentrated his attention on design works. Nevertheless, they occasionally continue to work together, as it happened in 2016, when they took part in the most important exhibition on street art held in China in the last decade, entitled \"Street Art, a Global View\". In addition to this fundamental exhibition, from 2007 to 2009 the crew took part in the most important Chinese graffiti exhibitions and competitions: \"China\u2019s First Graffiti Exhibition\" (Beijing, 20007), \"Re-dressed Vehicles Exposition\" (Beijing, 2007), \"Motor Trend Magazine Graffiti Exhibition\" (Beijing, 2007), \"798 Art District Graffiti Festival\" (Beijing, 2007), \"The Clash Show New Design Exhibition\" (Beijing, 2007), \"The Great Way of Graffiti Cultural Festival\" (Beijing, 2008), \"Beijing Happy Valley Hip-Hop Festival\" (Beijing, 2008), \"China International Cartoon & Game Expo\" (CCG EXPO), \"Shijingshan Cultural Festival Graffiti Show\" (Beijing, 2008), \"The 3rd China Beijing International Cultural & Creative Industry Expo\", \"The 2nd Graffiti Art Exhibition\" (Beijing, 2008), the \"Wall Lord Graffiti Battle\"(Wuhan, 2009), the \"Action for Seed Graffiti Jam\" (Shenzhen, 2009), etc.\nTheir first documented mural and their first recorded piece were made in December 2006. From that moment onward they created more than one hundred graffiti works and lots of graphic/digital design works (advertising posters, logos, books and CDs covers, prints on T-shirts, etc.). As usual for Chinese graffiti writers, the Kwanyin Clan engaged frequent collaborations with Chinese and foreign brands, especially with sports brands (Kappa, Lotto, Nike), electronic brands (Nokia, DELL, HP), and private clubs (VICS, Master Club, KTV, Gaga club, Club underground, Golf club, etc.). In these cases, their graffiti works were usually made on removable panels (once even on paper boxes, for the iMART in 2008) or indoor walls (of clubs or shops) using Latin letters and Old school Euro-Americans styles.\nThe Kwanyin Clan also collaborated with the Chinese government for the beautification of the metropolitan environment or to promote the image of a prosperous country in international competitions, as it happened in 2008 for the Beijing Olympics Games.\nIn addition to these legal activities for private companies and public institutions, the Kwanyin Clan was also involved in illegal bombing on the streets, especially in the 798 Art District and in Sanlitun in the city of Beijing. Their first documented bombing activity took place in April 2007 in the 798 Art District. This kind of activity usually consists of tagging up and/or throw-ups made in bubble letters filled with one or maximum two colours.\nBecause of their relevance in the Beijing graffiti panorama, the Kwanyin Clan is also the author of numerous murals and pieces in spray painting made on the most important halls of fame in Beijing: i) in the 798 Art District, ii) in the neighborhood of the Today Art Museum, and iii) near the China-Japan Friendship Hospital and the Beijing Institute of Fashion Technology, along a street that they named \"The Kwanyin Street\" (Guanyin Jie \u89c2\u97f3\u8857) because of the high number of pieces they made there.\nLike most of the Chinese crews, the primary aim of their art is the beautification of urban space (not rebellion), and self-expression. The majority of their unauthorized graffiti are apolitical or they are created to support the establishment and not to criticise it. Their attitude towards commissioned and paid graffiti is extremely positive and, in fact, they usually engage collaborations with foreign and domestic brands to promote their commercial campaigns. In their art (as in most of Chinese local creators), the paradox of turning an artistic language (graffiti art) that is expression of subcultures into mainstream, or even a commercial product, is evident.\nExamining the crew artistic production, it is possible to distinguish a good number of oeuvres characterised by the use of both Chinese and Western elements and/or techniques, exploring the fusion of graffiti art with China traditional aesthetics. With the exception of bombing activity and most of their commercial works, the use of Chinese characters together with Latin letters is a leitmotiv in their artistic production from their first mural onwards. From the beginning of their activity, Chinese characters are usually used for writing Chinese tags (especially in the forms of Guan or Guanyin characters), and from 2008 they have experimented with the use of Chinese characters instead of Latin letters in the creation of very elaborate charactering pieces. Another characteristic of their works is the presence of other visual elements that are easily recognisable as Chinese (such as pandas, dragons, bamboo and rocks, lotus flowers, pagodas, red fishes, Taoist deities, etc.), usually painted in form of comic puppet.\nBut, in addition to the use of Chinese writing and Chinese elements (common to other Chinese crews too), the peculiarity of the Kwanyin Clan is that in some particular works they also try to merge Euro-American graffiti practice with the ancient traditional arts of the Chinese literati (calligraphy, poetry, painting, and seal carving) or with uncommon media, for example ceramics, that are traditionally Chinese and highly appreciated by the Chinese literati.\nThe crew\u2019s main aim is the development of a notion of Chineseness in contemporary graffiti, as they have repeatedly stated and as their name dedicated to the Chinese \"Goddess of Mercy\" (Guanyin) alludes.\nThe name of the crew is significant because it clearly reveals the deep meaning of their art: \"Kwan-yin\" explicitly refers to the \"Bodhisattva Guanyin\" (Guanyin Pusa \u89c2\u97f3\u83e9\u8428), one of the most popular and well known deity in China. The crew itself points out that the story of the Bodhisattva was fundamental for the choice of their name: Guanyin, in fact, was introduced into China via the Silk Road and was initially presented in a male form, but when Buddhism rooted in China, the Bodhisattva was sinicized and subsequently transformed into a Chinese female form, which later became extremely popular all over the country. Kwanyin Clan\u2019s artistic aim is to present graffiti art in China as an alter ego of the Bodhisattva Guanyin: as it had happened for Guanyin, graffiti art came from abroad, but if it wants to become popular in China it has to sinicize its forms, undergoing deity own metamorphosis.\nTheir aim \"is committed to the dissemination and development of graffiti art in China\" in order to \"explore the fusion of graffiti art with Chinese culture\". For that reason, their art is usually defined \"Chinese Style Graffiti\" (Zhongguo tese de tuya yishu \u4e2d\u56fd\u7279\u8272\u7684\u6d82\u9e26\u827a\u672f) because it blends together the style of Western graffiti and the traditional Chinese aesthetics.\nThey usually use Chinese language to write their tags (especially in the forms of \"Guan\" \u89c2 [\u89c0] or \"Guanyin\" \u89c2\u97f3 [\u89c0\u97f3] characters) as well as to create elaborate charactering pieces, like for example in Shirupozhu \u52bf\u5982\u7834\u7af9 (With Irresistible Force, 2008), and Shengongyijiang \u795e\u5de5\u610f\u5320 (Ars Divina, 2010). Also the insertion of visual elements that are easily recognizable as Chinese is a constant in the Kwanyin Clan\u2019s works: pandas, dragons, bamboo and rocks, lotus flowers, pagodas, red fishes, Taoist deities, ping pong, mahjong, fans, etc. are extremely common in their oeuvres. In addition to the use of Chinese writing and Chinese visual cultural elements, the Kwanyin Clan also tried to merge Euro-American graffiti practice with Chinese traditional aesthetics and, in particular, with the ancient traditional arts of the Chinese literati (calligraphy, poetry and painting). This is a distinguishable feature of their art and perhaps one of the best interpretation of the notion of \"Chineseness\" in Chinese graffiti.\nIn their artworks, in fact, i) the use of calligraphic inscriptions is a recurring element; ii) the reference to Chinese classical poetry and \"sanjue\" (\u4e09\u7edd) notion (the reference to the Chinese indissoluble artistic triad of \"poetry, calligraphy and painting\" (shi shu hua \u8bd7\u4e66\u753b) that from the eighth century onward, in China have been known as the \"three perfections\", and they have been intimately connected in the minds of the \"Chinese literati\" (wen ren \u6587\u4eba), representing the basis of traditional Chinese culture (Sullivan 1974, 7) is not rare (in artworks like Shirupozhu, Shengongyijiang, and New Style); and iii) in addition to landscape painting, other Chinese traditional art forms has been reinterpreted, such as bamboo painting and archaic bronze vessels in New Style, bamboo and rocks painting in Bamboo, ceramics in Blue and white porcelain (2009-2010) and in Artwork made for the exhibition \"Street Art, a Global View\" (2016), paper-cutting in Passage of time, and jade disks carving in Situanheqi \u56db\u56e2\u548c\u6c14 (Four-cluster harmony, 2010).\nFor them, the \"sinicization\" process of graffiti art means not only the use of Chinese language and other Chinese visual elements but also drawing from China\u2019s classical tradition: this is their innovative notion of \"Chineseness\".", "@language": "en"}], "write:artistic-collective-type": [{"type": "customvocab:32", "property_id": 585, "property_label": "artistic collective type", "is_public": true, "@id": "https://www.wikidata.org/wiki/Q11964195", "o:label": "Crew"}], "wd:P276": [{"type": "uri", "property_id": 417, "property_label": "location", "is_public": true, "@id": "https://www.wikidata.org/entity/Q956", "o:label": "Beijing"}, {"type": "uri", "property_id": 417, "property_label": "location", "is_public": true, "@id": "https://www.wikidata.org/entity/Q1203470", "o:label": "798 Art Zone"}], "wd:P571": [{"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "2006-06", "@type": "http://www.w3.org/2001/XMLSchema#gYearMonth"}], "wd:P527": [{"type": "resource:item", "property_id": 435, "property_label": "has part", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/112", "value_resource_id": 112, "value_resource_name": "items", "url": null, "display_title": "EricTin", "thumbnail_url": "https://writeproject.dipintra.it/files/square/f1b3961de25228f7596e20f8d82a8ec70016ad8d.jpg", "thumbnail_title": "Erictin", "thumbnail_type": "image/jpeg"}, {"type": "resource:item", "property_id": 435, "property_label": "has part", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/121", "value_resource_id": 121, "value_resource_name": "items", "url": null, "display_title": "Natuo", "thumbnail_url": "https://writeproject.dipintra.it/files/square/2d729487df53b3cdfcdaa8a3d21287bd9c9aa30a.jpg", "thumbnail_title": "Natuo", "thumbnail_type": "image/jpeg"}, {"type": "resource:item", "property_id": 435, "property_label": "has part", "is_public": true, "@id": "https://writeproject.dipintra.it/api/items/1153", "value_resource_id": 1153, "value_resource_name": "items", "url": null, "display_title": "Yumi", "thumbnail_url": "https://writeproject.dipintra.it/files/square/9bb401f9606516de2554ce7d05995399a8a05d7f.jpg", "thumbnail_title": "Yumi", "thumbnail_type": "image/jpeg"}], "wd:P856": [{"type": "uri", "property_id": 415, "property_label": "website link", "is_public": true, "@id": "https://www.flickr.com/people/28329067@N06/", "o:label": "Flickr account"}, {"type": "uri", "property_id": 415, "property_label": "website link", "is_public": true, "@id": "https://www.youtube.com/watch?v=gpAJdXNKKJc", "o:label": "Video Youtube: \u89c2\u97f3(KWAN-YIN)crew - Chinese Style Graffiti"}, {"type": "uri", "property_id": 415, "property_label": "website link", "is_public": true, "@id": "https://www.instagram.com/kwan_clan/", "o:label": "Kwan clan Instagram account"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/1147", "@type": ["o:Item", "write:TraditionalArtwork"], "o:id": 1147, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/149", "o:id": 149}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/21", "o:id": 21}, "o:thumbnail": null, "o:title": "Travelers Among Mountains and Streams (\u6eaa\u5c71\u884c\u65c5\u56fe)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/fcebdf5e5e53bd7c34b1833742738e7c7b8a20e6.jpg", "medium": "https://writeproject.dipintra.it/files/medium/fcebdf5e5e53bd7c34b1833742738e7c7b8a20e6.jpg", "square": "https://writeproject.dipintra.it/files/square/fcebdf5e5e53bd7c34b1833742738e7c7b8a20e6.jpg"}, "o:created": {"@value": "2024-03-01T11:27:54+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T16:07:01+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/1168", "o:id": 1168}], "o:item_set": [], "o:site": [], "dcterms:title": [{"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "Travelers Among Mountains and Streams (\u6eaa\u5c71\u884c\u65c5\u56fe)", "@language": "en"}, {"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "Xi shan xing l\u00fc tu", "@language": "pny"}, {"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "\u8c3f\u5c71\u884c\u65c5\u5716", "@language": "zh"}], "dcterms:description": [{"type": "literal", "property_id": 4, "property_label": "Description", "is_public": true, "@value": "\"Travelers among Mountains and Streams\" is a large hanging scroll (ink and light color on silk, 206.3 x 103.3 cm, National Palace Museum, Taipei). The Author of Travelers Among Mountains and Streams is Fan Kuan \u8303\u5bec (ca. 950-1032), one of the most important Chinese landscape painters during the Song Dynasty. This painting is considered one of the finest monumental Chinese landscape paintings. It also became a model for later Chinese painters. Fan Kuan based his painting on the Daoist principle of becoming one with nature. The classic Chinese perspective of three planes is evident - near, middle (represented by water and mist), and far. Unlike earlier examples of Chinese landscape art, the grandeur of nature is the main theme, rather than merely providing a backdrop. A packhorse train can barely be seen emerging from a wood at the base of a towering precipice. The painting's style encompasses archaic conventions dating back to the Tang dynasty. The clusters of vegetation at the top of the tall mountain here are actually distant forests clinging to precarious perches. Running along the central axis of the scroll, the central mountain dominates the scene in a classic example of Northern Song monumental landscape painting. The rooftops of a building complex stand out in the right middle-ground. By the cluster of rocks in the right foreground is a path on which a mule train makes its way. A cascade as slender as silk falls from the heights above, culminating in the stream rushing down in eddies towards the foreground. From near to far, Fan Kuan has described with realistic detail the solemn grandeur of a majestic landscape. Fan Kuan rendered the mountains and slopes with jagged outline strokes and filled them with brush dabs like raindrops \u2013 techniques that highlight the monumental and eternal features of the mountains. To the right of the mule train, among the leaves, is the signature of Fan Kuan, a final touch by an artist to epitomize the insignificance of humans (including himself) compared to Nature. The signature was discovered by Li Lincan (\u674e\u9716\u71e6), a scholar at the National Palace Museum in Taipei, on August 5, 1958.\n\nSource: Iezzi, A., (2019) The Kwanyin Clan Modern Literati Graffiti Writers. An Aesthetic and Text Analysis of Their Main Artworks, Annali di Ca\u2019 Foscari. Serie orientale, n. 55, pp. 395-448: https://edizionicafoscari.unive.it/media/pdf/article/annali-di-ca-foscari-serie-orientale/2019/55/art-10.14277-AnnOr-2385-3042-2019-01-013_XWgnTc2.pdf", "@language": "en"}], "write:artwork-type": [{"type": "uri", "property_id": 505, "property_label": "artwork type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3305213", "o:label": "painting"}], "wd:P571": [{"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "1000", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "write:wikidata-resource": [{"type": "uri", "property_id": 567, "property_label": "wikidata resource", "is_public": true, "@id": "https://www.wikidata.org/entity/Q15907822", "o:label": "Travelers Among Mountains and Streams"}, {"type": "uri", "property_id": 567, "property_label": "wikidata resource", "is_public": true, "@id": "https://www.wikidata.org/entity/Q1278559", "o:label": "Fan Kuan"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/1148", "@type": ["o:Item", "write:TraditionalArtwork"], "o:id": 1148, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/149", "o:id": 149}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/21", "o:id": 21}, "o:thumbnail": null, "o:title": "A Thousand Li of Rivers and Mountains (\u5343\u91cc\u6c5f\u5c71\u56fe)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/f47fc672bc3599c0dbee089319709a9c550c64ed.jpg", "medium": "https://writeproject.dipintra.it/files/medium/f47fc672bc3599c0dbee089319709a9c550c64ed.jpg", "square": "https://writeproject.dipintra.it/files/square/f47fc672bc3599c0dbee089319709a9c550c64ed.jpg"}, "o:created": {"@value": "2024-03-01T11:29:59+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-12T16:06:49+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/1169", "o:id": 1169}], "o:item_set": [], "o:site": [], "dcterms:title": [{"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "A Thousand Li of Rivers and Mountains (\u5343\u91cc\u6c5f\u5c71\u56fe)", "@language": "en"}, {"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "Qianli Jiangshan tu", "@language": "pny"}, {"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "\u5343\u91cc\u6c5f\u5c71\u56fe", "@language": "zh"}], "dcterms:description": [{"type": "literal", "property_id": 4, "property_label": "Description", "is_public": true, "@value": "\"A Thousand Li of Rivers and Mountains\" was painted by Wang Ximeng \u738b\u5e0c\u5b5f (1096-1119), one of the most renowned court painters of the Northern Song period (960-1126). This painting is one of the largest in Chinese history, and the only Wang\u2019s surviving work. The painter used blue and green mineral colours for decoration, so this genre of painting is known as \u2018blue and green landscape\u2019 (qingl\u00fc shanshui \u9752\u7eff\u5c71\u6c34). This technique was a recognised form of painting as early as the seventh century. The painting, finished by Wang when he was only 18 in 1113, has been described as one of the greatest works of Chinese art. It is in the permanent collection of the Palace Museum in Beijing. The scroll's size is 51.5 by 1191.5 cm. The scenery in the painting was later identified as Mount Lu and Poyang Lake in Jiujiang. Wang Ximeng used typical technique of multiple perspectives, painting mountainous landscapes \"naturally separated into six sections by bridges and water\". Mountains were described as being \"in the style of Southern China\"; it can indicate that Wang Ximeng was from that region.\n\nSource: Iezzi, A., (2019) The Kwanyin Clan Modern Literati Graffiti Writers. An Aesthetic and Text Analysis of Their Main Artworks, Annali di Ca\u2019 Foscari. Serie orientale, n. 55, pp. 395-448: https://edizionicafoscari.unive.it/media/pdf/article/annali-di-ca-foscari-serie-orientale/2019/55/art-10.14277-AnnOr-2385-3042-2019-01-013_XWgnTc2.pdf", "@language": "en"}], "write:artwork-type": [{"type": "uri", "property_id": 505, "property_label": "artwork type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3305213", "o:label": "painting"}], "wd:P571": [{"type": "numeric:timestamp", "property_id": 431, "property_label": "date of creation", "is_public": true, "@value": "1113", "@type": "http://www.w3.org/2001/XMLSchema#gYear"}], "write:wikidata-resource": [{"type": "uri", "property_id": 567, "property_label": "wikidata resource", "is_public": true, "@id": "https://www.wikidata.org/entity/Q112971367", "o:label": "A Thousand Li of Rivers and Mountains"}, {"type": "uri", "property_id": 567, "property_label": "wikidata resource", "is_public": true, "@id": "https://www.wikidata.org/entity/Q626360", "o:label": "Wang Ximeng"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/1149", "@type": ["o:Item", "write:TraditionalArtwork"], "o:id": 1149, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/149", "o:id": 149}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/21", "o:id": 21}, "o:thumbnail": null, "o:title": "Dwelling in the Mountains (\u5c71\u5c45\u56fe)", "thumbnail_display_urls": {"large": null, "medium": null, "square": null}, "o:created": {"@value": "2024-03-01T12:56:54+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-13T08:52:26+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [], "o:item_set": [], "o:site": [], "dcterms:title": [{"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "Dwelling in the Mountains (\u5c71\u5c45\u56fe)", "@language": "en"}, {"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "sh\u0101nj\u016b t\u00fa", "@language": "pny"}, {"type": "literal", "property_id": 1, "property_label": "Title", "is_public": true, "@value": "\u5c71\u5c45\u5716", "@language": "zh"}], "dcterms:description": [{"type": "literal", "property_id": 4, "property_label": "Description", "is_public": true, "@value": "The Author of \"Dwelling in the Mountains\" is Qian Xuan \u94b1\u9009 (1235-1305), a Song loyalist painter during the last period of the Southern Song Dynasty (1127-1279) and of the first period of the Yuan Dynasty (1271-1368). \n\nSource: Iezzi, A., (2019) The Kwanyin Clan Modern Literati Graffiti Writers. An Aesthetic and Text Analysis of Their Main Artworks, Annali di Ca\u2019 Foscari. Serie orientale, n. 55, pp. 395-448: https://edizionicafoscari.unive.it/media/pdf/article/annali-di-ca-foscari-serie-orientale/2019/55/art-10.14277-AnnOr-2385-3042-2019-01-013_XWgnTc2.pdf", "@language": "en"}], "write:artwork-type": [{"type": "uri", "property_id": 505, "property_label": "artwork type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3305213", "o:label": "painting"}], "write:wikidata-resource": [{"type": "uri", "property_id": 567, "property_label": "wikidata resource", "is_public": true, "@id": "https://www.wikidata.org/entity/Q951585", "o:label": "Qian Xuan"}]}, {"@context": "https://writeproject.dipintra.it/api-context", "@id": "https://writeproject.dipintra.it/api/items/1151", "@type": ["o:Item", "write:DecorativeAndAppliedArt"], "o:id": 1151, "o:is_public": true, "o:owner": {"@id": "https://writeproject.dipintra.it/api/users/4", "o:id": 4}, "o:resource_class": {"@id": "https://writeproject.dipintra.it/api/resource_classes/139", "o:id": 139}, "o:resource_template": {"@id": "https://writeproject.dipintra.it/api/resource_templates/9", "o:id": 9}, "o:thumbnail": null, "o:title": "The Entire World Celebrates (\u666e\u5929\u540c\u5e86)", "thumbnail_display_urls": {"large": "https://writeproject.dipintra.it/files/large/c8d8f62ae9bf88c8e0a1bf058c0a07ad10b424f1.jpg", "medium": "https://writeproject.dipintra.it/files/medium/c8d8f62ae9bf88c8e0a1bf058c0a07ad10b424f1.jpg", "square": "https://writeproject.dipintra.it/files/square/c8d8f62ae9bf88c8e0a1bf058c0a07ad10b424f1.jpg"}, "o:created": {"@value": "2024-03-04T16:07:43+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:modified": {"@value": "2024-03-13T08:52:17+00:00", "@type": "http://www.w3.org/2001/XMLSchema#dateTime"}, "o:media": [{"@id": "https://writeproject.dipintra.it/api/media/1160", "o:id": 1160}], "o:item_set": [{"@id": "https://writeproject.dipintra.it/api/item_sets/1173", "o:id": 1173}], "o:site": [], "wd:P1476": [{"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "The Entire World Celebrates (\u666e\u5929\u540c\u5e86)", "@language": "en"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "Putian tong qing", "@language": "pny"}, {"type": "literal", "property_id": 421, "property_label": "title", "is_public": true, "@value": "\u666e\u5929\u540c\u5e86", "@language": "zh"}], "write:description": [{"type": "literal", "property_id": 560, "property_label": "description", "is_public": true, "@value": "The artwork \"Entire World Celebrates (Putian tong qing \u666e\u5929\u540c\u5e86)\" was the Kwanyin Clan\u2019s most successful experiment with distinctly Chinese yet modernised elements in a work commissioned by a foreign brand. It depicts a football fully hand-painted with tempera and marker, made on commission for Adidas for the 2010 FIFA World Cup advertising campaign. Yumi and Nat, the creators of the piece, covered the entire surface of the ball with traditional Chinese motifs, reworked in very personal ways. A curved line divides the ball into two parts and recreates the Taijitu \u592a\u6781\u56fe, the Taoist symbol par excellence, consisting of the stylized shape of a black and a white fish, intertwining in a whirling embrace. It represents the concept of the Tao (Dao \u9053), the source of being that regulates the development of the universe through the alternation of two complementary elements: yin \u9634, the dark-negative polarity represented by the black fish, and yang \u9633, the light-positive polarity represented by the white fish. \nThe two sections of the ball are filled with opposing motifs: one recalls the Euro-American graffiti tradition and the other the traditional Chinese culture, and these intertwine just like Yin and Yang, in a perfect fusion. On one side, Yumi creates an extreme grey and white wildstyle, while on the other, on a red background, Nat draws the zoomorphic image of the taotie \u9955\u992e, an archaic decorative motif typical of ritual bronzes from the Shang (c. 1600-1046 BC) and Zhou (1046-221 BC) dynasties, representing a frontal, symmetrical mask with various parts of different animals. Since, in the Chinese tradition, football \u2013 or cuju \u8e74\u97a0 \u2013 evoked a Zen meditative act, we instinctively thought of the image of the Taijitu, consisting of the stylised form of two fish, one white and one black, intertwining with each other, surrounded by the Eight Trigrams. This design formed the basis of the work, to which we then added the zoomorphic image of the taotie, which is a traditional Chinese decorative motif I am particularly fond of. Interestingly, this work is intended to recall the ancient Chinese game of football, cuju (lit. kicking a leather ball). According to FIFA, cuju is the earliest attested form of football in the world, and its invention dates from the Han dynasty (206 BC-220 AD). Initially, it was slightly different from the modern game: there were six holes on each side of the pitch with six goalkeepers, and each team, composed of six players plus the goalkeepers, had to score the highest number of goals. In the Tang dynasty, the holes turned into two goals on opposite sides of the field and the game resembled the current one. The Eight Trigrams are figures composed of three parallel lines, surrounding the Taijitu with a perfect octagon, each aligned to a cardinal point. The lines can be continuous or broken, indicating positive (yang) or negative (yin) polarity respectively. Each trigram corresponds to a natural element or phenomenon (sky, swamp, fire, thunder, wind, water, mountain and earth), and all together they symbolize the contents of the Book of Changes (Yijing), one of the fundamental texts of traditional Chinese knowledge. The wildstyle within one of the two sections of the ball recalls the linearity of the trigrams. Reference to the World Cup in the image is evident: the two sections of the Taijitu ideally represent the two football teams (the yin and the yang) that share and mix on the same pitch (the circle), thus generating a continuous action (the Tao).", "@language": "en"}], "write:product-type": [{"type": "uri", "property_id": 506, "property_label": "product type", "is_public": true, "@id": "https://www.wikidata.org/entity/Q262090", "o:label": "football"}], "wd:P186": [{"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q175166", "o:label": "tempera"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q127418", "o:label": "ink"}, {"type": "uri", "property_id": 428, "property_label": "made by material", "is_public": true, "@id": "https://www.wikidata.org/entity/Q286", "o:label": "leather"}], "wd:P2079": [{"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q2743156", "o:label": "permanent marker"}, {"type": "uri", "property_id": 438, "property_label": "fabrication method", "is_public": true, "@id": "https://www.wikidata.org/entity/Q614467", "o:label": "brush"}], "wd:P180": [{"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q758058", "o:label": "Taotie"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q190393", "o:label": "Tao"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q62744", "o:label": "yin and yang"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q262090", "o:label": "football"}, {"type": "uri", "property_id": 420, "property_label": "depicts", "is_public": true, "@id": "https://www.wikidata.org/entity/Q138208", "o:label": "cuju"}], "wd:P462": [{"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q3142", "o:label": "red"}, {"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23445", "o:label": "black"}, {"type": "uri", "property_id": 534, "property_label": "color", "is_public": true, "@id": "https://www.wikidata.org/entity/Q23444", "o:label": "white"}, {"type": "uri", "property_id": 534, "prop